{
  "description": "Create a video",
  "operationId": "createVideo",
  "parameters": [],
  "requestBody": {
    "content": {
      "application/json": {
        "schema": {
          "description": "Parameters for creating a new video generation job.",
          "properties": {
            "input_reference": {
              "description": "Optional image reference that guides generation.",
              "format": "binary",
              "type": "string"
            },
            "model": {
              "description": "The video generation model to use. Defaults to `sora-2`.",
              "enum": [
                "sora-2",
                "sora-2-pro"
              ],
              "type": "string"
            },
            "prompt": {
              "description": "Text prompt that describes the video to generate.",
              "maxLength": 32000,
              "minLength": 1,
              "type": "string"
            },
            "seconds": {
              "description": "Clip duration in seconds. Defaults to 4 seconds.",
              "enum": [
                "4",
                "8",
                "12"
              ],
              "type": "string"
            },
            "size": {
              "description": "Output resolution formatted as width x height. Defaults to 720x1280.",
              "enum": [
                "720x1280",
                "1280x720",
                "1024x1792",
                "1792x1024"
              ],
              "type": "string"
            }
          },
          "required": [
            "prompt"
          ],
          "title": "Create video request",
          "type": "object"
        }
      },
      "multipart/form-data": {
        "schema": {
          "description": "Parameters for creating a new video generation job.",
          "properties": {
            "input_reference": {
              "description": "Optional image reference that guides generation.",
              "format": "binary",
              "type": "string"
            },
            "model": {
              "description": "The video generation model to use. Defaults to `sora-2`.",
              "enum": [
                "sora-2",
                "sora-2-pro"
              ],
              "type": "string"
            },
            "prompt": {
              "description": "Text prompt that describes the video to generate.",
              "maxLength": 32000,
              "minLength": 1,
              "type": "string"
            },
            "seconds": {
              "description": "Clip duration in seconds. Defaults to 4 seconds.",
              "enum": [
                "4",
                "8",
                "12"
              ],
              "type": "string"
            },
            "size": {
              "description": "Output resolution formatted as width x height. Defaults to 720x1280.",
              "enum": [
                "720x1280",
                "1280x720",
                "1024x1792",
                "1792x1024"
              ],
              "type": "string"
            }
          },
          "required": [
            "prompt"
          ],
          "title": "Create video request",
          "type": "object"
        }
      }
    }
  },
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "description": "Structured information describing a generated video job.",
            "properties": {
              "completed_at": {
                "anyOf": [
                  {
                    "description": "Unix timestamp (seconds) for when the job completed, if finished.",
                    "type": "integer"
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "created_at": {
                "description": "Unix timestamp (seconds) for when the job was created.",
                "type": "integer"
              },
              "error": {
                "anyOf": [
                  {
                    "description": "Error payload that explains why generation failed, if applicable.",
                    "properties": {
                      "code": {
                        "type": "string"
                      },
                      "message": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "code",
                      "message"
                    ],
                    "type": "object"
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "expires_at": {
                "anyOf": [
                  {
                    "description": "Unix timestamp (seconds) for when the downloadable assets expire, if set.",
                    "type": "integer"
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "id": {
                "description": "Unique identifier for the video job.",
                "type": "string"
              },
              "model": {
                "description": "The video generation model that produced the job.",
                "enum": [
                  "sora-2",
                  "sora-2-pro"
                ],
                "type": "string"
              },
              "object": {
                "default": "video",
                "description": "The object type, which is always `video`.",
                "enum": [
                  "video"
                ],
                "type": "string"
              },
              "progress": {
                "description": "Approximate completion percentage for the generation task.",
                "type": "integer"
              },
              "remixed_from_video_id": {
                "anyOf": [
                  {
                    "description": "Identifier of the source video if this video is a remix.",
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "seconds": {
                "description": "Duration of the generated clip in seconds.",
                "enum": [
                  "4",
                  "8",
                  "12"
                ],
                "type": "string"
              },
              "size": {
                "description": "The resolution of the generated video.",
                "enum": [
                  "720x1280",
                  "1280x720",
                  "1024x1792",
                  "1792x1024"
                ],
                "type": "string"
              },
              "status": {
                "description": "Current lifecycle status of the video job.",
                "enum": [
                  "queued",
                  "in_progress",
                  "completed",
                  "failed"
                ],
                "type": "string"
              }
            },
            "required": [
              "id",
              "object",
              "model",
              "status",
              "progress",
              "created_at",
              "completed_at",
              "expires_at",
              "size",
              "seconds",
              "remixed_from_video_id",
              "error"
            ],
            "title": "Video job",
            "type": "object"
          }
        }
      },
      "description": "Success"
    }
  },
  "summary": "Create video",
  "tags": [
    "Videos"
  ]
}
