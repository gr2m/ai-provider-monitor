{
  "description": "Creates a custom voice.",
  "operationId": "createVoice",
  "requestBody": {
    "content": {
      "multipart/form-data": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "audio_sample": {
              "description": "The sample audio recording file. Maximum size is 10 MiB.\n\nSupported MIME types:\n`audio/mpeg`, `audio/wav`, `audio/x-wav`, `audio/ogg`, `audio/aac`, `audio/flac`, `audio/webm`, `audio/mp4`.\n",
              "format": "binary",
              "type": "string"
            },
            "consent": {
              "description": "The consent recording ID (for example, `cons_1234`).",
              "type": "string"
            },
            "name": {
              "description": "The name of the new voice.",
              "type": "string"
            }
          },
          "required": [
            "name",
            "audio_sample",
            "consent"
          ],
          "type": "object"
        }
      }
    },
    "required": true
  },
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "additionalProperties": false,
            "description": "A custom voice that can be used for audio output.",
            "properties": {
              "created_at": {
                "description": "The Unix timestamp (in seconds) for when the voice was created.",
                "type": "integer"
              },
              "id": {
                "description": "The voice identifier, which can be referenced in API endpoints.",
                "type": "string"
              },
              "name": {
                "description": "The name of the voice.",
                "type": "string"
              },
              "object": {
                "description": "The object type, which is always `audio.voice`.",
                "enum": [
                  "audio.voice"
                ],
                "type": "string"
              }
            },
            "required": [
              "object",
              "id",
              "name",
              "created_at"
            ],
            "title": "Voice",
            "type": "object"
          }
        }
      },
      "description": "OK"
    }
  },
  "summary": "Create voice",
  "tags": [
    "Audio"
  ]
}
