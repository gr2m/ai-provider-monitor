{
  "description": "Delete a consent recording that was uploaded for creating custom voices.\n\nSee the [custom voices guide](/docs/guides/text-to-speech#custom-voices). Custom voices are limited to eligible customers.\n",
  "operationId": "deleteVoiceConsent",
  "parameters": [
    {
      "description": "The ID of the consent recording to delete.",
      "in": "path",
      "name": "consent_id",
      "required": true,
      "schema": {
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "deleted": {
                "type": "boolean"
              },
              "id": {
                "description": "The consent recording identifier.",
                "example": "cons_1234",
                "type": "string"
              },
              "object": {
                "enum": [
                  "audio.voice_consent"
                ],
                "type": "string"
              }
            },
            "required": [
              "id",
              "object",
              "deleted"
            ],
            "type": "object"
          }
        }
      },
      "description": "OK"
    }
  },
  "summary": "Deletes a voice consent recording.",
  "tags": [
    "Audio"
  ]
}
