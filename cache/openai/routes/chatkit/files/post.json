{
  "description": "Upload a ChatKit file",
  "operationId": "UploadFileMethod",
  "parameters": [],
  "requestBody": {
    "content": {
      "application/json": {
        "schema": {
          "description": "Parameters for uploading an attachment to the active ChatKit session.",
          "properties": {
            "file": {
              "description": "Binary file contents to store with the ChatKit session. Supports PDFs and PNG, JPG, JPEG, GIF, or WEBP images.",
              "format": "binary",
              "type": "string"
            }
          },
          "required": [
            "file"
          ],
          "title": "Upload file request",
          "type": "object"
        }
      },
      "multipart/form-data": {
        "schema": {
          "description": "Parameters for uploading an attachment to the active ChatKit session.",
          "properties": {
            "file": {
              "description": "Binary file contents to store with the ChatKit session. Supports PDFs and PNG, JPG, JPEG, GIF, or WEBP images.",
              "format": "binary",
              "type": "string"
            }
          },
          "required": [
            "file"
          ],
          "title": "Upload file request",
          "type": "object"
        }
      }
    }
  },
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "anyOf": [
              {
                "description": "Metadata for a non-image file uploaded through ChatKit.",
                "properties": {
                  "id": {
                    "description": "Unique identifier for the uploaded file.",
                    "type": "string"
                  },
                  "mime_type": {
                    "anyOf": [
                      {
                        "description": "MIME type reported for the uploaded file. Defaults to null when unknown.",
                        "type": "string"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "name": {
                    "anyOf": [
                      {
                        "description": "Original filename supplied by the uploader. Defaults to null when unnamed.",
                        "type": "string"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "type": {
                    "default": "file",
                    "description": "Type discriminator that is always `file`.",
                    "enum": [
                      "file"
                    ],
                    "type": "string"
                  },
                  "upload_url": {
                    "anyOf": [
                      {
                        "description": "Signed URL for downloading the uploaded file. Defaults to null when no download link is available.",
                        "type": "string"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  }
                },
                "required": [
                  "id",
                  "type",
                  "name",
                  "mime_type",
                  "upload_url"
                ],
                "title": "Uploaded file",
                "type": "object"
              },
              {
                "description": "Metadata for an image uploaded through ChatKit.",
                "properties": {
                  "id": {
                    "description": "Unique identifier for the uploaded image.",
                    "type": "string"
                  },
                  "mime_type": {
                    "description": "MIME type of the uploaded image.",
                    "type": "string"
                  },
                  "name": {
                    "anyOf": [
                      {
                        "description": "Original filename for the uploaded image. Defaults to null when unnamed.",
                        "type": "string"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "preview_url": {
                    "description": "Preview URL that can be rendered inline for the image.",
                    "type": "string"
                  },
                  "type": {
                    "default": "image",
                    "description": "Type discriminator that is always `image`.",
                    "enum": [
                      "image"
                    ],
                    "type": "string"
                  },
                  "upload_url": {
                    "anyOf": [
                      {
                        "description": "Signed URL for downloading the uploaded image. Defaults to null when no download link is available.",
                        "type": "string"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  }
                },
                "required": [
                  "id",
                  "type",
                  "mime_type",
                  "name",
                  "preview_url",
                  "upload_url"
                ],
                "title": "Uploaded image",
                "type": "object"
              }
            ],
            "description": "Represents either a file or image attachment.",
            "discriminator": {
              "propertyName": "type"
            }
          }
        }
      },
      "description": "Success"
    }
  },
  "summary": "Upload file to ChatKit"
}
