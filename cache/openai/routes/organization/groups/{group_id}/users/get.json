{
  "description": "Lists the users assigned to a group.",
  "operationId": "list-group-users",
  "parameters": [
    {
      "description": "The ID of the group to inspect.",
      "in": "path",
      "name": "group_id",
      "required": true,
      "schema": {
        "type": "string"
      }
    },
    {
      "description": "A limit on the number of users to be returned. Limit can range between 0 and 1000, and the default is 100.\n",
      "in": "query",
      "name": "limit",
      "required": false,
      "schema": {
        "default": 100,
        "maximum": 1000,
        "minimum": 0,
        "type": "integer"
      }
    },
    {
      "description": "A cursor for use in pagination. Provide the ID of the last user from the previous list response to retrieve the next page.\n",
      "in": "query",
      "name": "after",
      "required": false,
      "schema": {
        "type": "string"
      }
    },
    {
      "description": "Specifies the sort order of users in the list.",
      "in": "query",
      "name": "order",
      "required": false,
      "schema": {
        "default": "desc",
        "enum": [
          "asc",
          "desc"
        ],
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "description": "Paginated list of user objects returned when inspecting group membership.",
            "properties": {
              "data": {
                "description": "Users in the current page.",
                "items": {
                  "description": "Represents an individual `user` within an organization.",
                  "properties": {
                    "added_at": {
                      "description": "The Unix timestamp (in seconds) of when the user was added.",
                      "type": "integer"
                    },
                    "email": {
                      "description": "The email address of the user",
                      "type": "string"
                    },
                    "id": {
                      "description": "The identifier, which can be referenced in API endpoints",
                      "type": "string"
                    },
                    "name": {
                      "description": "The name of the user",
                      "type": "string"
                    },
                    "object": {
                      "description": "The object type, which is always `organization.user`",
                      "enum": [
                        "organization.user"
                      ],
                      "type": "string"
                    },
                    "role": {
                      "description": "`owner` or `reader`",
                      "enum": [
                        "owner",
                        "reader"
                      ],
                      "type": "string"
                    }
                  },
                  "required": [
                    "object",
                    "id",
                    "name",
                    "email",
                    "role",
                    "added_at"
                  ],
                  "type": "object"
                },
                "type": "array"
              },
              "has_more": {
                "description": "Whether more users are available when paginating.",
                "type": "boolean"
              },
              "next": {
                "anyOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ],
                "description": "Cursor to fetch the next page of results, or `null` when no further users are available."
              },
              "object": {
                "description": "Always `list`.",
                "enum": [
                  "list"
                ],
                "type": "string"
              }
            },
            "required": [
              "object",
              "data",
              "has_more",
              "next"
            ],
            "type": "object"
          }
        }
      },
      "description": "Group users listed successfully."
    }
  },
  "summary": "List group users",
  "tags": [
    "Group users"
  ]
}
