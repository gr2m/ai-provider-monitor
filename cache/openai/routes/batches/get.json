              "first_id": {
                "example": "batch_abc123",
                "type": "string"
              },
              "has_more": {
                "type": "boolean"
              },
              "last_id": {
                "example": "batch_abc456",
                "type": "string"
              },
              "object": {
                "enum": [
                  "list"
