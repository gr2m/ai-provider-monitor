{
  "operationId": "CreateSkill",
  "parameters": [],
  "requestBody": {
    "content": {
      "application/json": {
        "schema": {
          "$ref": "#/components/schemas/CreateSkillBody"
        }
      },
      "multipart/form-data": {
        "schema": {
          "$ref": "#/components/schemas/CreateSkillBody"
        }
      }
    }
  },
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/SkillResource"
          }
        }
      },
      "description": "Success"
    }
  },
  "summary": "Create a new skill.",
  "tags": [
    "Skills"
  ],
  "components": {
    "schemas": {
      "CreateSkillBody": {
        "description": "Uploads a skill either as a directory (multipart `files[]`) or as a single zip file.",
        "properties": {
          "files": {
            "oneOf": [
              {
                "description": "Skill files to upload (directory upload) or a single zip file.",
                "items": {
                  "format": "binary",
                  "type": "string"
                },
                "maxItems": 500,
                "type": "array"
              },
              {
                "description": "Skill zip file to upload.",
                "format": "binary",
                "type": "string"
              }
            ]
          }
        },
        "required": [
          "files"
        ],
        "title": "Create skill request",
        "type": "object"
      },
      "SkillResource": {
        "properties": {
          "created_at": {
            "description": "Unix timestamp (seconds) for when the skill was created.",
            "type": "integer"
          },
          "default_version": {
            "description": "Default version for the skill.",
            "type": "string"
          },
          "description": {
            "description": "Description of the skill.",
            "type": "string"
          },
          "id": {
            "description": "Unique identifier for the skill.",
            "type": "string"
          },
          "latest_version": {
            "description": "Latest version for the skill.",
            "type": "string"
          },
          "name": {
            "description": "Name of the skill.",
            "type": "string"
          },
          "object": {
            "default": "skill",
            "description": "The object type, which is `skill`.",
            "enum": [
              "skill"
            ],
            "type": "string"
          }
        },
        "required": [
          "id",
          "object",
          "name",
          "description",
          "created_at",
          "default_version",
          "latest_version"
        ],
        "type": "object"
      }
    }
  }
}
