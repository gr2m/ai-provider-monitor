{
  "operationId": "ListSkillVersions",
  "parameters": [
    {
      "description": "The identifier of the skill.",
      "in": "path",
      "name": "skill_id",
      "required": true,
      "schema": {
        "example": "skill_123",
        "type": "string"
      }
    },
    {
      "description": "Number of versions to retrieve.",
      "in": "query",
      "name": "limit",
      "required": false,
      "schema": {
        "maximum": 100,
        "minimum": 0,
        "type": "integer"
      }
    },
    {
      "description": "Sort order of results by version number.",
      "in": "query",
      "name": "order",
      "required": false,
      "schema": {
        "$ref": "#/components/schemas/OrderEnum"
      }
    },
    {
      "description": "The skill version ID to start after.",
      "in": "query",
      "name": "after",
      "required": false,
      "schema": {
        "example": "skillver_123",
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/SkillVersionListResource"
          }
        }
      },
      "description": "Success"
    }
  },
  "summary": "List skill versions for a skill.",
  "tags": [
    "Skills"
  ],
  "components": {
    "schemas": {
      "OrderEnum": {
        "enum": [
          "asc",
          "desc"
        ],
        "type": "string"
      },
      "SkillVersionListResource": {
        "properties": {
          "data": {
            "description": "A list of items",
            "items": {
              "$ref": "#/components/schemas/SkillVersionResource"
            },
            "type": "array"
          },
          "first_id": {
            "anyOf": [
              {
                "description": "The ID of the first item in the list.",
                "type": "string"
              },
              {
                "type": "null"
              }
            ]
          },
          "has_more": {
            "description": "Whether there are more items available.",
            "type": "boolean"
          },
          "last_id": {
            "anyOf": [
              {
                "description": "The ID of the last item in the list.",
                "type": "string"
              },
              {
                "type": "null"
              }
            ]
          },
          "object": {
            "default": "list",
            "description": "The type of object returned, must be `list`.",
            "enum": [
              "list"
            ],
            "type": "string"
          }
        },
        "required": [
          "object",
          "data",
          "first_id",
          "last_id",
          "has_more"
        ],
        "type": "object"
      },
      "SkillVersionResource": {
        "properties": {
          "created_at": {
            "description": "Unix timestamp (seconds) for when the version was created.",
            "type": "integer"
          },
          "description": {
            "description": "Description of the skill version.",
            "type": "string"
          },
          "id": {
            "description": "Unique identifier for the skill version.",
            "type": "string"
          },
          "name": {
            "description": "Name of the skill version.",
            "type": "string"
          },
          "object": {
            "default": "skill.version",
            "description": "The object type, which is `skill.version`.",
            "enum": [
              "skill.version"
            ],
            "type": "string"
          },
          "skill_id": {
            "description": "Identifier of the skill for this version.",
            "type": "string"
          },
          "version": {
            "description": "Version number for this skill.",
            "type": "string"
          }
        },
        "required": [
          "object",
          "id",
          "skill_id",
          "version",
          "created_at",
          "name",
          "description"
        ],
        "type": "object"
      }
    }
  }
}
