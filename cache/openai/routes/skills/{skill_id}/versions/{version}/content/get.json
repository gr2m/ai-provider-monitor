{
  "operationId": "GetSkillVersionContent",
  "parameters": [
    {
      "description": "The identifier of the skill.",
      "in": "path",
      "name": "skill_id",
      "required": true,
      "schema": {
        "example": "skill_123",
        "type": "string"
      }
    },
    {
      "description": "The skill version number.",
      "in": "path",
      "name": "version",
      "required": true,
      "schema": {
        "description": "The skill version number.",
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "type": "string"
          }
        },
        "application/zip": {
          "schema": {
            "format": "binary",
            "type": "string"
          }
        }
      },
      "description": "The skill zip bundle."
    }
  },
  "summary": "Download a skill version zip bundle.",
  "tags": [
    "Skills"
  ]
}
