{
  "operationId": "GetSkillVersion",
  "parameters": [
    {
      "description": "The identifier of the skill.",
      "in": "path",
      "name": "skill_id",
      "required": true,
      "schema": {
        "example": "skill_123",
        "type": "string"
      }
    },
    {
      "description": "The version number to retrieve.",
      "in": "path",
      "name": "version",
      "required": true,
      "schema": {
        "description": "The version number to retrieve.",
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "created_at": {
                "description": "Unix timestamp (seconds) for when the version was created.",
                "type": "integer"
              },
              "description": {
                "description": "Description of the skill version.",
                "type": "string"
              },
              "id": {
                "description": "Unique identifier for the skill version.",
                "type": "string"
              },
              "name": {
                "description": "Name of the skill version.",
                "type": "string"
              },
              "object": {
                "default": "skill.version",
                "description": "The object type, which is `skill.version`.",
                "enum": [
                  "skill.version"
                ],
                "type": "string"
              },
              "skill_id": {
                "description": "Identifier of the skill for this version.",
                "type": "string"
              },
              "version": {
                "description": "Version number for this skill.",
                "type": "string"
              }
            },
            "required": [
              "object",
              "id",
              "skill_id",
              "version",
              "created_at",
              "name",
              "description"
            ],
            "type": "object"
          }
        }
      },
      "description": "Success"
    }
  },
  "summary": "Get a specific skill version.",
  "tags": [
    "Skills"
  ]
}
