{
  "operationId": "GetSkill",
  "parameters": [
    {
      "description": "The identifier of the skill to retrieve.",
      "in": "path",
      "name": "skill_id",
      "required": true,
      "schema": {
        "example": "skill_123",
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "created_at": {
                "description": "Unix timestamp (seconds) for when the skill was created.",
                "type": "integer"
              },
              "default_version": {
                "description": "Default version for the skill.",
                "type": "string"
              },
              "description": {
                "description": "Description of the skill.",
                "type": "string"
              },
              "id": {
                "description": "Unique identifier for the skill.",
                "type": "string"
              },
              "latest_version": {
                "description": "Latest version for the skill.",
                "type": "string"
              },
              "name": {
                "description": "Name of the skill.",
                "type": "string"
              },
              "object": {
                "default": "skill",
                "description": "The object type, which is `skill`.",
                "enum": [
                  "skill"
                ],
                "type": "string"
              }
            },
            "required": [
              "id",
              "object",
              "name",
              "description",
              "created_at",
              "default_version",
              "latest_version"
            ],
            "type": "object"
          }
        }
      },
      "description": "Success"
    }
  },
  "summary": "Get a skill by its ID.",
  "tags": [
    "Skills"
  ]
}
