      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "deleted": {
                "type": "boolean"
              },
              "id": {
                "type": "string"
              },
              "object": {
                "default": "skill.deleted",
                "enum": [
                  "skill.deleted"
                ],
                "type": "string"
              }
            },
            "required": [
              "object",
              "deleted",
              "id"
            ],
            "type": "object"
          }
        }
      },
