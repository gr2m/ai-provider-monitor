{
  "operationId": "GetSkillContent",
  "parameters": [
    {
      "description": "The identifier of the skill to download.",
      "in": "path",
      "name": "skill_id",
      "required": true,
      "schema": {
        "example": "skill_123",
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "type": "string"
          }
        },
        "application/zip": {
          "schema": {
            "format": "binary",
            "type": "string"
          }
        }
      },
      "description": "The skill zip bundle."
    }
  },
  "summary": "Download a skill zip bundle by its ID.",
  "tags": [
    "Skills"
  ]
}
