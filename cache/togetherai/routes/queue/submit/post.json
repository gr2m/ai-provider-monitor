{
  "description": "Submit a new job to the queue for asynchronous processing. Jobs are\nprocessed in strict priority order (higher priority first, FIFO within\nthe same priority). Returns a request ID that can be used to poll status\nor cancel the job.\n",
  "operationId": "submitQueueJob",
  "requestBody": {
    "content": {
      "application/json": {
        "schema": {
          "properties": {
            "info": {
              "additionalProperties": true,
              "description": "Arbitrary JSON metadata stored with the job and returned in status\nresponses. The model and system may add or update keys during\nprocessing.\n",
              "type": "object"
            },
            "model": {
              "description": "Required model identifier",
              "type": "string"
            },
            "payload": {
              "additionalProperties": true,
              "description": "Freeform model input. Passed unchanged to the model. Contents are model-specific.",
              "type": "object"
            },
            "priority": {
              "default": 0,
              "description": "Job priority. Higher values are processed first (strict priority\nordering). Jobs with equal priority are processed in submission\norder (FIFO).\n",
              "type": "integer"
            }
          },
          "required": [
            "model",
            "payload"
          ],
          "type": "object"
        }
      }
    },
    "description": "Job request",
    "required": true
  },
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "error": {
                "properties": {
                  "code": {
                    "description": "Machine-readable error code",
                    "type": "string"
                  },
                  "message": {
                    "description": "Human-readable error message",
                    "type": "string"
                  },
                  "param": {
                    "description": "The parameter that caused the error, if applicable",
                    "type": "string"
                  },
                  "type": {
                    "description": "Error category (e.g. \"invalid_request_error\", \"not_found_error\")",
                    "type": "string"
                  }
                },
                "type": "object"
              },
              "requestId": {
                "description": "Unique identifier for the submitted job. Use this to poll status or cancel.",
                "type": "string"
              }
            },
            "type": "object"
          }
        }
      },
      "description": "Successfully queued request"
    },
    "400": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "code": {
                "description": "Machine-readable error code",
                "type": "string"
              },
              "message": {
                "description": "Human-readable error message",
                "type": "string"
              },
              "param": {
                "description": "The parameter that caused the error, if applicable",
                "type": "string"
              },
              "type": {
                "description": "Error category (e.g. \"invalid_request_error\", \"not_found_error\")",
                "type": "string"
              }
            },
            "type": "object"
          }
        }
      },
      "description": "Invalid request"
    },
    "401": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "code": {
                "description": "Machine-readable error code",
                "type": "string"
              },
              "message": {
                "description": "Human-readable error message",
                "type": "string"
              },
              "param": {
                "description": "The parameter that caused the error, if applicable",
                "type": "string"
              },
              "type": {
                "description": "Error category (e.g. \"invalid_request_error\", \"not_found_error\")",
                "type": "string"
              }
            },
            "type": "object"
          }
        }
      },
      "description": "Unauthorized"
    },
    "500": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "code": {
                "description": "Machine-readable error code",
                "type": "string"
              },
              "message": {
                "description": "Human-readable error message",
                "type": "string"
              },
              "param": {
                "description": "The parameter that caused the error, if applicable",
                "type": "string"
              },
              "type": {
                "description": "Error category (e.g. \"invalid_request_error\", \"not_found_error\")",
                "type": "string"
              }
            },
            "type": "object"
          }
        }
      },
      "description": "Internal server error"
    }
  },
  "summary": "Submit a queued job",
  "tags": [
    "Queue"
  ]
}
