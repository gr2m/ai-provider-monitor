{
  "description": "Retrieve logs from a deployment, optionally filtered by replica ID.",
  "parameters": [
    {
      "in": "path",
      "name": "id",
      "required": true,
      "schema": {
        "description": "Deployment ID or name",
        "type": "string"
      }
    },
    {
      "in": "query",
      "name": "replica_id",
      "required": false,
      "schema": {
        "description": "Replica ID to filter logs",
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/DeploymentLogs"
          }
        }
      },
      "description": "Deployment logs"
    },
    "404": {
      "content": {
        "application/json": {
          "schema": {
            "type": "object"
          }
        }
      },
      "description": "Deployment not found"
    },
    "500": {
      "content": {
        "application/json": {
          "schema": {
            "type": "object"
          }
        }
      },
      "description": "Internal server error"
    }
  },
  "summary": "Get logs for a deployment",
  "tags": [
    "Deployments"
  ],
  "components": {
    "schemas": {
      "DeploymentLogs": {
        "properties": {
          "lines": {
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    }
  }
}
