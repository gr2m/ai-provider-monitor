{
  "description": "Retrieve all volumes in your project",
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "data": {
                "description": "Data is the array of volume items",
                "items": {
                  "properties": {
                    "content": {
                      "allOf": [
                        {
                          "properties": {
                            "source_prefix": {
                              "description": "SourcePrefix is the file path prefix for the content to be preloaded into the volume",
                              "example": "models/",
                              "type": "string"
                            },
                            "type": {
                              "description": "Type is the content type (currently only \"files\" is supported which allows preloading files uploaded via Files API into the volume)",
                              "enum": [
                                "files"
                              ],
                              "example": "files",
                              "type": "string"
                            }
                          },
                          "type": "object"
                        }
                      ],
                      "description": "Content specifies the content that will be preloaded to this volume"
                    },
                    "created_at": {
                      "description": "CreatedAt is the ISO8601 timestamp when this volume was created",
                      "type": "string"
                    },
                    "id": {
                      "description": "ID is the unique identifier for this volume",
                      "type": "string"
                    },
                    "name": {
                      "description": "Name is the name of the volume",
                      "type": "string"
                    },
                    "object": {
                      "const": "volume",
                      "description": "The object type, which is always `volume`."
                    },
                    "type": {
                      "allOf": [
                        {
                          "enum": [
                            "readOnly"
                          ],
                          "type": "string"
                        }
                      ],
                      "description": "Type is the volume type (e.g., \"readOnly\")"
                    },
                    "updated_at": {
                      "description": "UpdatedAt is the ISO8601 timestamp when this volume was last updated",
                      "type": "string"
                    }
                  },
                  "type": "object"
                },
                "type": "array"
              },
              "object": {
                "const": "list",
                "description": "The object type, which is always `list`."
              }
            },
            "type": "object"
          }
        }
      },
      "description": "List of volumes"
    },
    "500": {
      "content": {
        "application/json": {
          "schema": {
            "type": "object"
          }
        }
      },
      "description": "Internal server error"
    }
  },
  "summary": "Get the list of project volumes",
  "tags": [
    "DeploymentsVolumes"
  ]
}
