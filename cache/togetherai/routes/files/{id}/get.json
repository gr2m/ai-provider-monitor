{
  "description": "Retrieve the metadata for a single uploaded data file.",
  "parameters": [
    {
      "in": "path",
      "name": "id",
      "required": true,
      "schema": {
        "description": "The ID of the file to retrieve",
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/FileResponse"
          }
        }
      },
      "description": "File retrieved successfully"
    }
  },
  "summary": "Retrieve file metadata",
  "tags": [
    "Files"
  ],
  "components": {
    "schemas": {
      "FileResponse": {
        "description": "Structured information describing a file uploaded to Together.",
        "properties": {
          "FileType": {
            "$ref": "#/components/schemas/FileType",
            "description": "The type of the file such as `jsonl`, `csv`, or `parquet`."
          },
          "Processed": {
            "description": "Whether the file has been parsed and analyzed for correctness for fine-tuning.",
            "type": "boolean"
          },
          "bytes": {
            "description": "The number of bytes in the file.",
            "type": "integer"
          },
          "created_at": {
            "description": "The timestamp when the file was created.",
            "type": "integer"
          },
          "filename": {
            "description": "The name of the file as it was uploaded.",
            "example": "my_file.jsonl",
            "type": "string"
          },
          "id": {
            "description": "ID of the file.",
            "type": "string"
          },
          "object": {
            "const": "file",
            "description": "The object type, which is always `file`."
          },
          "purpose": {
            "$ref": "#/components/schemas/FilePurpose",
            "description": "The purpose of the file as it was uploaded."
          }
        },
        "required": [
          "id",
          "object",
          "created_at",
          "filename",
          "bytes",
          "purpose",
          "FileType",
          "Processed"
        ],
        "type": "object"
      },
      "FileType": {
        "default": "jsonl",
        "description": "The type of the file",
        "enum": [
          "csv",
          "jsonl",
          "parquet"
        ],
        "example": "jsonl",
        "type": "string"
      },
      "FilePurpose": {
        "description": "The purpose of the file",
        "enum": [
          "fine-tune",
          "eval",
          "eval-sample",
          "eval-output",
          "eval-summary",
          "batch-generated",
          "batch-api"
        ],
        "example": "fine-tune",
        "type": "string"
      }
    }
  }
}
