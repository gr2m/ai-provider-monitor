{
  "description": "Get the contents of a single uploaded data file.",
  "parameters": [
    {
      "in": "path",
      "name": "id",
      "required": true,
      "schema": {
        "description": "The ID of the file to get the content of",
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "text/plain": {
          "schema": {
            "format": "binary",
            "type": "string"
          }
        }
      },
      "description": "File content retrieved successfully"
    },
    "500": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/ErrorData"
          }
        }
      },
      "description": "Internal Server Error"
    }
  },
  "summary": "Get file contents",
  "tags": [
    "Files"
  ],
  "components": {
    "schemas": {
      "ErrorData": {
        "properties": {
          "error": {
            "properties": {
              "code": {
                "default": null,
                "nullable": true,
                "type": "string"
              },
              "message": {
                "nullable": false,
                "type": "string"
              },
              "param": {
                "default": null,
                "nullable": true,
                "type": "string"
              },
              "type": {
                "nullable": false,
                "type": "string"
              }
            },
            "required": [
              "type",
              "message"
            ],
            "type": "object"
          }
        },
        "required": [
          "error"
        ],
        "type": "object"
      }
    }
  }
}
