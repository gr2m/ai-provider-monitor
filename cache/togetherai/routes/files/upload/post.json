{
  "description": "Upload a file with specified purpose, file name, and file type.",
  "requestBody": {
    "content": {
      "multipart/form-data": {
        "schema": {
          "properties": {
            "file": {
              "description": "The content of the file being uploaded",
              "format": "binary",
              "type": "string"
            },
            "file_name": {
              "description": "The name of the file being uploaded",
              "example": "dataset.csv",
              "type": "string"
            },
            "file_type": {
              "$ref": "#/components/schemas/FileType"
            },
            "purpose": {
              "$ref": "#/components/schemas/FilePurpose"
            }
          },
          "required": [
            "purpose",
            "file_name",
            "file"
          ],
          "type": "object"
        }
      }
    },
    "required": true
  },
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/FileResponse"
          }
        }
      },
      "description": "File uploaded successfully"
    },
    "400": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/ErrorData"
          }
        }
      },
      "description": "Bad Request"
    },
    "401": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/ErrorData"
          }
        }
      },
      "description": "Unauthorized"
    },
    "500": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/ErrorData"
          }
        }
      },
      "description": "Internal Server Error"
    }
  },
  "summary": "Upload a file",
  "tags": [
    "Files"
  ],
  "components": {
    "schemas": {
      "FileType": {
        "default": "jsonl",
        "description": "The type of the file",
        "enum": [
          "csv",
          "jsonl",
          "parquet"
        ],
        "example": "jsonl",
        "type": "string"
      },
      "FilePurpose": {
        "description": "The purpose of the file",
        "enum": [
          "fine-tune",
          "eval",
          "eval-sample",
          "eval-output",
          "eval-summary",
          "batch-generated",
          "batch-api"
        ],
        "example": "fine-tune",
        "type": "string"
      },
      "FileResponse": {
        "description": "Structured information describing a file uploaded to Together.",
        "properties": {
          "FileType": {
            "$ref": "#/components/schemas/FileType",
            "description": "The type of the file such as `jsonl`, `csv`, or `parquet`."
          },
          "Processed": {
            "description": "Whether the file has been parsed and analyzed for correctness for fine-tuning.",
            "type": "boolean"
          },
          "bytes": {
            "description": "The number of bytes in the file.",
            "type": "integer"
          },
          "created_at": {
            "description": "The timestamp when the file was created.",
            "type": "integer"
          },
          "filename": {
            "description": "The name of the file as it was uploaded.",
            "example": "my_file.jsonl",
            "type": "string"
          },
          "id": {
            "description": "ID of the file.",
            "type": "string"
          },
          "object": {
            "const": "file",
            "description": "The object type, which is always `file`."
          },
          "purpose": {
            "$ref": "#/components/schemas/FilePurpose",
            "description": "The purpose of the file as it was uploaded."
          }
        },
        "required": [
          "id",
          "object",
          "created_at",
          "filename",
          "bytes",
          "purpose",
          "FileType",
          "Processed"
        ],
        "type": "object"
      },
      "ErrorData": {
        "properties": {
          "error": {
            "properties": {
              "code": {
                "default": null,
                "nullable": true,
                "type": "string"
              },
              "message": {
                "nullable": false,
                "type": "string"
              },
              "param": {
                "default": null,
                "nullable": true,
                "type": "string"
              },
              "type": {
                "nullable": false,
                "type": "string"
              }
            },
            "required": [
              "type",
              "message"
            ],
            "type": "object"
          }
        },
        "required": [
          "error"
        ],
        "type": "object"
      }
    }
  }
}
