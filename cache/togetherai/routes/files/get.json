{
  "description": "List the metadata for all uploaded data files.",
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "data": {
                "items": {
                  "properties": {
                    "FileType": {
                      "default": "jsonl",
                      "description": "The type of the file",
                      "enum": [
                        "csv",
                        "jsonl",
                        "parquet"
                      ],
                      "example": "jsonl",
                      "type": "string"
                    },
                    "LineCount": {
                      "type": "integer"
                    },
                    "Processed": {
                      "type": "boolean"
                    },
                    "bytes": {
                      "example": 2664,
                      "type": "integer"
                    },
                    "created_at": {
                      "example": 1715021438,
                      "type": "integer"
                    },
                    "filename": {
                      "example": "my_file.jsonl",
                      "type": "string"
                    },
                    "id": {
                      "type": "string"
                    },
                    "object": {
                      "example": "file",
                      "type": "string"
                    },
                    "purpose": {
                      "description": "The purpose of the file",
                      "enum": [
                        "fine-tune",
                        "eval",
                        "eval-sample",
                        "eval-output",
                        "eval-summary",
                        "batch-generated",
                        "batch-api"
                      ],
                      "example": "fine-tune",
                      "type": "string"
                    }
                  },
                  "required": [
                    "id",
                    "object",
                    "created_at",
                    "filename",
                    "bytes",
                    "purpose",
                    "FileType",
                    "Processed",
                    "LineCount"
                  ],
                  "type": "object"
                },
                "type": "array"
              }
            },
            "required": [
              "data"
            ],
            "type": "object"
          }
        }
      },
      "description": "List of files"
    }
  },
  "summary": "List all files",
  "tags": [
    "Files"
  ]
}
