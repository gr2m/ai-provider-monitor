{
  "description": "Retrieve information about a specific GPU cluster.",
  "operationId": "GPUClusterService_Get",
  "parameters": [
    {
      "in": "path",
      "name": "cluster_id",
      "required": true,
      "schema": {
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "cluster_id": {
                "type": "string"
              },
              "cluster_name": {
                "type": "string"
              },
              "cluster_type": {
                "enum": [
                  "KUBERNETES",
                  "SLURM"
                ]
              },
              "control_plane_nodes": {
                "items": {
                  "properties": {
                    "host_name": {
                      "type": "string"
                    },
                    "memory_gib": {
                      "type": "number"
                    },
                    "network": {
                      "type": "string"
                    },
                    "node_id": {
                      "type": "string"
                    },
                    "node_name": {
                      "type": "string"
                    },
                    "num_cpu_cores": {
                      "type": "integer"
                    },
                    "status": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "node_id",
                    "node_name",
                    "status",
                    "host_name",
                    "num_cpu_cores",
                    "memory_gib",
                    "network"
                  ],
                  "type": "object"
                },
                "type": "array"
              },
              "driver_version": {
                "enum": [
                  "CUDA_12_5_555",
                  "CUDA_12_6_560",
                  "CUDA_12_6_565",
                  "CUDA_12_8_570"
                ]
              },
              "duration_hours": {
                "type": "integer"
              },
              "gpu_type": {
                "enum": [
                  "H100_SXM",
                  "H200_SXM",
                  "RTX_6000_PCI",
                  "L40_PCIE",
                  "B200_SXM",
                  "H100_SXM_INF"
                ]
              },
              "gpu_worker_nodes": {
                "items": {
                  "properties": {
                    "host_name": {
                      "type": "string"
                    },
                    "memory_gib": {
                      "type": "number"
                    },
                    "networks": {
                      "items": {
                        "type": "string"
                      },
                      "type": "array"
                    },
                    "node_id": {
                      "type": "string"
                    },
                    "node_name": {
                      "type": "string"
                    },
                    "num_cpu_cores": {
                      "type": "integer"
                    },
                    "num_gpus": {
                      "type": "integer"
                    },
                    "status": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "node_id",
                    "node_name",
                    "status",
                    "host_name",
                    "num_cpu_cores",
                    "num_gpus",
                    "memory_gib",
                    "networks"
                  ],
                  "type": "object"
                },
                "type": "array"
              },
              "kube_config": {
                "type": "string"
              },
              "num_gpus": {
                "type": "integer"
              },
              "region": {
                "type": "string"
              },
              "status": {
                "description": "Current status of the GPU cluster.",
                "enum": [
                  "WaitingForControlPlaneNodes",
                  "WaitingForDataPlaneNodes",
                  "WaitingForSubnet",
                  "WaitingForSharedVolume",
                  "InstallingDrivers",
                  "RunningAcceptanceTests",
                  "Paused",
                  "OnDemandComputePaused",
                  "Ready",
                  "Degraded",
                  "Deleting"
                ]
              },
              "volumes": {
                "items": {
                  "properties": {
                    "size_tib": {
                      "type": "integer"
                    },
                    "status": {
                      "type": "string"
                    },
                    "volume_id": {
                      "type": "string"
                    },
                    "volume_name": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "volume_id",
                    "volume_name",
                    "size_tib",
                    "status"
                  ],
                  "type": "object"
                },
                "type": "array"
              }
            },
            "required": [
              "cluster_id",
              "cluster_type",
              "region",
              "gpu_type",
              "cluster_name",
              "duration_hours",
              "driver_version",
              "volumes",
              "status",
              "control_plane_nodes",
              "gpu_worker_nodes",
              "kube_config",
              "num_gpus"
            ],
            "type": "object"
          }
        }
      },
      "description": "OK"
    }
  },
  "summary": "Get GPU cluster by cluster ID",
  "tags": [
    "GPUClusterService"
  ]
}
