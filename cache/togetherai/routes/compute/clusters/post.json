{
  "description": "Create an Instant Cluster on Together's high-performance GPU clusters.\nWith features like on-demand scaling, long-lived resizable high-bandwidth shared DC-local storage,\nKubernetes and Slurm cluster flavors, a REST API, and Terraform support,\nyou can run workloads flexibly without complex infrastructure management.\n",
  "operationId": "GPUClusterService_Create",
  "requestBody": {
    "content": {
      "application/json": {
        "schema": {
          "$ref": "#/components/schemas/GPUClusterCreateRequest"
        }
      }
    },
    "required": true
  },
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/GPUClusterInfo"
          }
        }
      },
      "description": "OK"
    }
  },
  "summary": "Create GPU Cluster",
  "tags": [
    "GPUClusterService"
  ],
  "components": {
    "schemas": {
      "GPUClusterCreateRequest": {
        "description": "GPU Cluster create request",
        "properties": {
          "billing_type": {
            "description": "RESERVED billing types allow you to specify the duration of the cluster reservation via the duration_days field.\nON_DEMAND billing types will give you ownership of the cluster until you delete it.\n",
            "enum": [
              "RESERVED",
              "ON_DEMAND"
            ],
            "type": "string"
          },
          "cluster_name": {
            "description": "Name of the GPU cluster.",
            "type": "string"
          },
          "cluster_type": {
            "description": "Type of cluster to create.",
            "enum": [
              "KUBERNETES",
              "SLURM"
            ],
            "type": "string"
          },
          "driver_version": {
            "description": "NVIDIA driver version to use in the cluster.",
            "enum": [
              "CUDA_12_5_555",
              "CUDA_12_6_560",
              "CUDA_12_6_565",
              "CUDA_12_8_570"
            ],
            "type": "string"
          },
          "duration_days": {
            "description": "Duration in days to keep the cluster running.",
            "type": "integer"
          },
          "gpu_type": {
            "description": "Type of GPU to use in the cluster",
            "enum": [
              "H100_SXM",
              "H200_SXM",
              "RTX_6000_PCI",
              "L40_PCIE",
              "B200_SXM",
              "H100_SXM_INF"
            ],
            "type": "string"
          },
          "num_gpus": {
            "description": "Number of GPUs to allocate in the cluster. This must be multiple of 8. For example, 8, 16 or 24",
            "type": "integer"
          },
          "region": {
            "description": "Region to create the GPU cluster in. Usable regions can be found from `client.clusters.list_regions()`",
            "type": "string"
          },
          "shared_volume": {
            "$ref": "#/components/schemas/GPUClustersSharedVolumeCreateRequest",
            "description": "Inline configuration to create a shared volume with the cluster creation."
          },
          "volume_id": {
            "description": "ID of an existing volume to use with the cluster creation.",
            "type": "string"
          }
        },
        "required": [
          "region",
          "gpu_type",
          "num_gpus",
          "cluster_name",
          "driver_version",
          "billing_type"
        ],
        "type": "object"
      },
      "GPUClusterInfo": {
        "properties": {
          "cluster_id": {
            "type": "string"
          },
          "cluster_name": {
            "type": "string"
          },
          "cluster_type": {
            "description": "Type of cluster.",
            "enum": [
              "KUBERNETES",
              "SLURM"
            ]
          },
          "control_plane_nodes": {
            "items": {
              "$ref": "#/components/schemas/GPUClusterControlPlaneNode"
            },
            "type": "array"
          },
          "driver_version": {
            "enum": [
              "CUDA_12_5_555",
              "CUDA_12_6_560",
              "CUDA_12_6_565",
              "CUDA_12_8_570"
            ]
          },
          "duration_hours": {
            "type": "integer"
          },
          "gpu_type": {
            "enum": [
              "H100_SXM",
              "H200_SXM",
              "RTX_6000_PCI",
              "L40_PCIE",
              "B200_SXM",
              "H100_SXM_INF"
            ]
          },
          "gpu_worker_nodes": {
            "items": {
              "$ref": "#/components/schemas/GPUClusterGPUWorkerNode"
            },
            "type": "array"
          },
          "kube_config": {
            "type": "string"
          },
          "num_gpus": {
            "type": "integer"
          },
          "region": {
            "type": "string"
          },
          "status": {
            "description": "Current status of the GPU cluster.",
            "enum": [
              "WaitingForControlPlaneNodes",
              "WaitingForDataPlaneNodes",
              "WaitingForSubnet",
              "WaitingForSharedVolume",
              "InstallingDrivers",
              "RunningAcceptanceTests",
              "Paused",
              "OnDemandComputePaused",
              "Ready",
              "Degraded",
              "Deleting"
            ]
          },
          "volumes": {
            "items": {
              "$ref": "#/components/schemas/GPUClusterVolume"
            },
            "type": "array"
          }
        },
        "required": [
          "cluster_id",
          "cluster_type",
          "region",
          "gpu_type",
          "cluster_name",
          "duration_hours",
          "driver_version",
          "volumes",
          "status",
          "control_plane_nodes",
          "gpu_worker_nodes",
          "kube_config",
          "num_gpus"
        ],
        "type": "object"
      },
      "GPUClustersSharedVolumeCreateRequest": {
        "properties": {
          "region": {
            "description": "Region name. Usable regions can be found from `client.clusters.list_regions()`",
            "type": "string"
          },
          "size_tib": {
            "description": "Volume size in whole tebibytes (TiB).",
            "type": "integer"
          },
          "volume_name": {
            "description": "Customizable name of the volume to create.",
            "type": "string"
          }
        },
        "required": [
          "volume_name",
          "size_tib",
          "region"
        ],
        "type": "object"
      },
      "GPUClusterControlPlaneNode": {
        "properties": {
          "host_name": {
            "type": "string"
          },
          "memory_gib": {
            "type": "number"
          },
          "network": {
            "type": "string"
          },
          "node_id": {
            "type": "string"
          },
          "node_name": {
            "type": "string"
          },
          "num_cpu_cores": {
            "type": "integer"
          },
          "status": {
            "type": "string"
          }
        },
        "required": [
          "node_id",
          "node_name",
          "status",
          "host_name",
          "num_cpu_cores",
          "memory_gib",
          "network"
        ],
        "type": "object"
      },
      "GPUClusterGPUWorkerNode": {
        "properties": {
          "host_name": {
            "type": "string"
          },
          "memory_gib": {
            "type": "number"
          },
          "networks": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "node_id": {
            "type": "string"
          },
          "node_name": {
            "type": "string"
          },
          "num_cpu_cores": {
            "type": "integer"
          },
          "num_gpus": {
            "type": "integer"
          },
          "status": {
            "type": "string"
          }
        },
        "required": [
          "node_id",
          "node_name",
          "status",
          "host_name",
          "num_cpu_cores",
          "num_gpus",
          "memory_gib",
          "networks"
        ],
        "type": "object"
      },
      "GPUClusterVolume": {
        "properties": {
          "size_tib": {
            "type": "integer"
          },
          "status": {
            "type": "string"
          },
          "volume_id": {
            "type": "string"
          },
          "volume_name": {
            "type": "string"
          }
        },
        "required": [
          "volume_id",
          "volume_name",
          "size_tib",
          "status"
        ],
        "type": "object"
      }
    }
  }
}
