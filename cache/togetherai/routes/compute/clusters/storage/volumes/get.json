{
  "description": "List all shared volumes.",
  "operationId": "SharedVolumeService_List",
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "volumes": {
                "items": {
                  "properties": {
                    "size_tib": {
                      "description": "Size of the volume in whole tebibytes (TiB).",
                      "type": "integer"
                    },
                    "status": {
                      "description": "Deployment status of the volume.",
                      "enum": [
                        "available",
                        "bound",
                        "provisioning"
                      ],
                      "type": "string"
                    },
                    "volume_id": {
                      "description": "ID of the volume.",
                      "type": "string"
                    },
                    "volume_name": {
                      "description": "Provided name of the volume.",
                      "type": "string"
                    }
                  },
                  "required": [
                    "volume_id",
                    "volume_name",
                    "size_tib",
                    "status"
                  ],
                  "type": "object"
                },
                "type": "array"
              }
            },
            "required": [
              "volumes"
            ],
            "type": "object"
          }
        }
      },
      "description": "OK"
    }
  },
  "summary": "List all shared volumes.",
  "tags": [
    "SharedVolumeService"
  ]
}
