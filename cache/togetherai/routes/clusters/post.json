{
  "operationId": "GPUClusterService_Create",
  "requestBody": {
    "content": {
      "application/json": {
        "schema": {
          "description": "GPU Cluster create request",
          "properties": {
            "billing_type": {
              "enum": [
                "RESERVED",
                "ON_DEMAND"
              ],
              "type": "string"
            },
            "cluster_name": {
              "description": "Name of the GPU cluster.",
              "type": "string"
            },
            "cluster_type": {
              "enum": [
                "KUBERNETES",
                "SLURM"
              ],
              "type": "string"
            },
            "driver_version": {
              "description": "NVIDIA driver version to use in the cluster.",
              "enum": [
                "CUDA_12_5_555",
                "CUDA_12_6_560",
                "CUDA_12_6_565",
                "CUDA_12_8_570"
              ],
              "type": "string"
            },
            "duration_days": {
              "description": "Duration in days to keep the cluster running.",
              "type": "integer"
            },
            "gpu_type": {
              "description": "Type of GPU to use in the cluster",
              "enum": [
                "H100_SXM",
                "H200_SXM",
                "RTX_6000_PCI",
                "L40_PCIE",
                "B200_SXM",
                "H100_SXM_INF"
              ],
              "type": "string"
            },
            "num_gpus": {
              "description": "Number of GPUs to allocate in the cluster. This must be multiple of 8. For example, 8, 16 or 24",
              "type": "integer"
            },
            "region": {
              "description": "Region to create the GPU cluster in. Valid values are us-central-8 and us-central-4.",
              "enum": [
                "us-central-8",
                "us-central-4"
              ],
              "type": "string"
            },
            "shared_volume": {
              "properties": {
                "region": {
                  "description": "Region name. Usable regions can be found from `client.clusters.list_regions()`",
                  "type": "string"
                },
                "size_tib": {
                  "description": "Volume size in whole tebibytes (TiB).",
                  "type": "integer"
                },
                "volume_name": {
                  "type": "string"
                }
              },
              "required": [
                "volume_name",
                "size_tib",
                "region"
              ],
              "type": "object"
            },
            "volume_id": {
              "type": "string"
            }
          },
          "required": [
            "region",
            "gpu_type",
            "num_gpus",
            "cluster_name",
            "duration_days",
            "driver_version",
            "billing_type"
          ],
          "type": "object"
        }
      }
    },
    "required": true
  },
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "cluster_id": {
                "type": "string"
              }
            },
            "required": [
              "cluster_id"
            ],
            "type": "object"
          }
        }
      },
      "description": "OK"
    }
  },
  "summary": "Create GPU Cluster",
  "tags": [
    "GPUClusterService"
  ]
}
