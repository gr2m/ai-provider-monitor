{
  "description": "Lists all training sessions.",
  "operationId": "listTrainingSessions",
  "parameters": [
    {
      "in": "query",
      "name": "status",
      "required": false,
      "schema": {
        "$ref": "#/components/schemas/RL.TrainingSessionStatus",
        "description": "Status of the training sessions to list"
      }
    },
    {
      "in": "query",
      "name": "limit",
      "required": false,
      "schema": {
        "default": "20",
        "description": "Maximum number of sessions to return (1-100), defaults to 20",
        "format": "int32",
        "type": "integer"
      }
    },
    {
      "in": "query",
      "name": "offset",
      "required": false,
      "schema": {
        "default": "0",
        "description": "Number of sessions to skip",
        "format": "int32",
        "type": "integer"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/RL.TrainingSessionsListResponse"
          }
        }
      },
      "description": "List of training sessions"
    },
    "default": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/RpcStatus"
          }
        }
      },
      "description": "An unexpected error response."
    }
  },
  "summary": "List training sessions",
  "tags": [
    "RL"
  ],
  "components": {
    "schemas": {
      "RL.TrainingSessionStatus": {
        "default": "TRAINING_SESSION_STATUS_UNSPECIFIED",
        "description": "Status of the training session",
        "enum": [
          "TRAINING_SESSION_STATUS_UNSPECIFIED",
          "TRAINING_SESSION_STATUS_CREATING",
          "TRAINING_SESSION_STATUS_RUNNING",
          "TRAINING_SESSION_STATUS_STOPPED",
          "TRAINING_SESSION_STATUS_STOPPING",
          "TRAINING_SESSION_STATUS_ERROR",
          "TRAINING_SESSION_STATUS_EXPIRED"
        ],
        "type": "string"
      },
      "RL.TrainingSessionsListResponse": {
        "properties": {
          "data": {
            "description": "List of training sessions",
            "items": {
              "$ref": "#/components/schemas/RL.TrainingSession",
              "type": "object"
            },
            "type": "array"
          },
          "meta": {
            "$ref": "#/components/schemas/RL.ListMeta",
            "description": "Pagination metadata"
          }
        },
        "type": "object"
      },
      "RpcStatus": {
        "properties": {
          "code": {
            "format": "int32",
            "type": "integer"
          },
          "details": {
            "items": {
              "$ref": "#/components/schemas/ProtobufAny",
              "type": "object"
            },
            "type": "array"
          },
          "message": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "RL.TrainingSession": {
        "properties": {
          "base_model": {
            "description": "Base model used for the training session",
            "example": "meta-llama/Meta-Llama-3-8B-Instruct",
            "type": "string"
          },
          "checkpoint_id": {
            "description": "Checkpoint ID to use for the training session",
            "type": "string"
          },
          "created_at": {
            "description": "Timestamp when the training session was created",
            "format": "date-time",
            "type": "string"
          },
          "lora_config": {
            "$ref": "#/components/schemas/RL.LoraConfig"
          },
          "session_id": {
            "description": "ID of the training session",
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/RL.TrainingSessionStatus"
          },
          "step": {
            "default": "0",
            "description": "Current training step",
            "format": "uint64",
            "type": "string"
          },
          "updated_at": {
            "description": "Timestamp when the training session was last updated",
            "format": "date-time",
            "type": "string"
          }
        },
        "type": "object"
      },
      "RL.ListMeta": {
        "properties": {
          "limit": {
            "description": "Maximum number of items returned per page",
            "example": 20,
            "format": "int32",
            "type": "integer"
          },
          "offset": {
            "description": "Number of items skipped",
            "example": 0,
            "format": "int32",
            "type": "integer"
          },
          "total": {
            "description": "Total number of items matching the filter",
            "example": 42,
            "format": "int64",
            "type": "string"
          }
        },
        "type": "object"
      },
      "ProtobufAny": {
        "additionalProperties": {},
        "properties": {
          "@type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "RL.LoraConfig": {
        "description": "LoRA adapter configuration",
        "properties": {
          "alpha": {
            "default": "16",
            "description": "Alpha of the LoRA adapter",
            "example": 16,
            "format": "int64",
            "type": "integer"
          },
          "dropout": {
            "default": "0.05",
            "description": "Dropout of the LoRA adapter",
            "example": 0.05,
            "format": "float",
            "type": "number"
          },
          "rank": {
            "default": "8",
            "description": "Rank of the LoRA adapter",
            "example": 8,
            "format": "int64",
            "type": "integer"
          }
        },
        "type": "object"
      }
    }
  }
}
