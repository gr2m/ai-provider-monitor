{
  "description": "Lists all training sessions.",
  "operationId": "listTrainingSessions",
  "parameters": [
    {
      "in": "query",
      "name": "status",
      "required": false,
      "schema": {
        "default": "TRAINING_SESSION_STATUS_UNSPECIFIED",
        "description": "Status of the training session",
        "enum": [
          "TRAINING_SESSION_STATUS_UNSPECIFIED",
          "TRAINING_SESSION_STATUS_CREATING",
          "TRAINING_SESSION_STATUS_RUNNING",
          "TRAINING_SESSION_STATUS_STOPPED",
          "TRAINING_SESSION_STATUS_STOPPING",
          "TRAINING_SESSION_STATUS_ERROR",
          "TRAINING_SESSION_STATUS_EXPIRED"
        ],
        "type": "string"
      }
    },
    {
      "in": "query",
      "name": "limit",
      "required": false,
      "schema": {
        "default": "20",
        "description": "Maximum number of sessions to return (1-100), defaults to 20",
        "format": "int32",
        "type": "integer"
      }
    },
    {
      "in": "query",
      "name": "offset",
      "required": false,
      "schema": {
        "default": "0",
        "description": "Number of sessions to skip",
        "format": "int32",
        "type": "integer"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "data": {
                "description": "List of training sessions",
                "items": {
                  "type": "object",
                  "properties": {
                    "base_model": {
                      "description": "Base model used for the training session",
                      "example": "meta-llama/Meta-Llama-3-8B-Instruct",
                      "type": "string"
                    },
                    "checkpoint_id": {
                      "description": "Checkpoint ID to use for the training session",
                      "type": "string"
                    },
                    "created_at": {
                      "description": "Timestamp when the training session was created",
                      "format": "date-time",
                      "type": "string"
                    },
                    "lora_config": {
                      "description": "LoRA adapter configuration",
                      "properties": {
                        "alpha": {
                          "default": "16",
                          "description": "Alpha of the LoRA adapter",
                          "example": 16,
                          "format": "int64",
                          "type": "integer"
                        },
                        "dropout": {
                          "default": "0.05",
                          "description": "Dropout of the LoRA adapter",
                          "example": 0.05,
                          "format": "float",
                          "type": "number"
                        },
                        "rank": {
                          "default": "8",
                          "description": "Rank of the LoRA adapter",
                          "example": 8,
                          "format": "int64",
                          "type": "integer"
                        }
                      },
                      "type": "object"
                    },
                    "session_id": {
                      "description": "ID of the training session",
                      "type": "string"
                    },
                    "status": {
                      "default": "TRAINING_SESSION_STATUS_UNSPECIFIED",
                      "description": "Status of the training session",
                      "enum": [
                        "TRAINING_SESSION_STATUS_UNSPECIFIED",
                        "TRAINING_SESSION_STATUS_CREATING",
                        "TRAINING_SESSION_STATUS_RUNNING",
                        "TRAINING_SESSION_STATUS_STOPPED",
                        "TRAINING_SESSION_STATUS_STOPPING",
                        "TRAINING_SESSION_STATUS_ERROR",
                        "TRAINING_SESSION_STATUS_EXPIRED"
                      ],
                      "type": "string"
                    },
                    "step": {
                      "default": "0",
                      "description": "Current training step",
                      "format": "uint64",
                      "type": "string"
                    },
                    "updated_at": {
                      "description": "Timestamp when the training session was last updated",
                      "format": "date-time",
                      "type": "string"
                    }
                  }
                },
                "type": "array"
              },
              "meta": {
                "description": "Pagination metadata",
                "properties": {
                  "limit": {
                    "description": "Maximum number of items returned per page",
                    "example": 20,
                    "format": "int32",
                    "type": "integer"
                  },
                  "offset": {
                    "description": "Number of items skipped",
                    "example": 0,
                    "format": "int32",
                    "type": "integer"
                  },
                  "total": {
                    "description": "Total number of items matching the filter",
                    "example": 42,
                    "format": "int64",
                    "type": "string"
                  }
                },
                "type": "object"
              }
            },
            "type": "object"
          }
        }
      },
      "description": "List of training sessions"
    },
    "default": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "code": {
                "format": "int32",
                "type": "integer"
              },
              "details": {
                "items": {
                  "type": "object",
                  "additionalProperties": {},
                  "properties": {
                    "@type": {
                      "type": "string"
                    }
                  }
                },
                "type": "array"
              },
              "message": {
                "type": "string"
              }
            },
            "type": "object"
          }
        }
      },
      "description": "An unexpected error response."
    }
  },
  "summary": "List training sessions",
  "tags": [
    "RL"
  ]
}
