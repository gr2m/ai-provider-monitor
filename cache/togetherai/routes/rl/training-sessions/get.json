{
  "description": "Lists all training sessions.",
  "operationId": "listTrainingSessions",
  "parameters": [
    {
      "in": "query",
      "name": "status",
      "required": false,
      "type": {
        "default": "TRAINING_SESSION_STATUS_UNSPECIFIED",
        "description": "Status of the training session",
        "enum": [
          "TRAINING_SESSION_STATUS_UNSPECIFIED",
          "TRAINING_SESSION_STATUS_CREATING",
          "TRAINING_SESSION_STATUS_RUNNING",
          "TRAINING_SESSION_STATUS_STOPPED",
          "TRAINING_SESSION_STATUS_STOPPING",
          "TRAINING_SESSION_STATUS_ERROR"
        ],
        "type": "string"
      }
    },
    {
      "default": 20,
      "description": "Maximum number of sessions to return (1-100)",
      "in": "query",
      "name": "limit",
      "required": false,
      "type": "integer"
    },
    {
      "default": 0,
      "description": "Number of sessions to skip",
      "in": "query",
      "name": "offset",
      "required": false,
      "type": "integer"
    }
  ],
  "responses": {
    "200": {
      "description": "List of training sessions",
      "schema": {
        "items": {
          "type": "object",
          "properties": {
            "base_model": {
              "description": "Base model used for the training session",
              "example": "meta-llama/Meta-Llama-3-8B-Instruct",
              "type": "string"
            },
            "created_at": {
              "description": "Timestamp when the training session was created",
              "format": "date-time",
              "type": "string"
            },
            "lora_config": {
              "description": "LoRA adapter configuration",
              "properties": {
                "alpha": {
                  "default": 16,
                  "description": "Alpha of the LoRA adapter",
                  "type": "integer"
                },
                "dropout": {
                  "default": 0.05,
                  "description": "Dropout of the LoRA adapter",
                  "type": "number"
                },
                "rank": {
                  "default": 8,
                  "description": "Rank of the LoRA adapter",
                  "type": "integer"
                }
              },
              "type": "object"
            },
            "lr_scheduler_config": {
              "description": "Learning rate scheduler configuration",
              "properties": {
                "linear": {
                  "description": "Linear learning rate scheduler configuration",
                  "properties": {
                    "params": {
                      "description": "Linear learning rate scheduler parameters",
                      "properties": {
                        "lr_min": {
                          "default": 0,
                          "description": "Minimum learning rate at the end of linear decay",
                          "type": "number"
                        },
                        "warmup_steps": {
                          "default": 100,
                          "description": "Number of warmup steps",
                          "type": "integer"
                        }
                      },
                      "type": "object"
                    }
                  },
                  "type": "object"
                }
              },
              "type": "object"
            },
            "optimizer_config": {
              "description": "Optimizer configuration. If omitted, defaults to AdamW with default parameters.",
              "properties": {
                "adamw": {
                  "description": "AdamW optimizer configuration",
                  "properties": {
                    "params": {
                      "description": "AdamW optimizer parameters",
                      "properties": {
                        "beta1": {
                          "default": 0.9,
                          "description": "First moment decay rate",
                          "type": "number"
                        },
                        "beta2": {
                          "default": 0.95,
                          "description": "Second moment decay rate",
                          "type": "number"
                        },
                        "eps": {
                          "default": 1e-8,
                          "description": "Epsilon for numerical stability",
                          "type": "number"
                        },
                        "lr": {
                          "default": 0.0001,
                          "description": "Learning rate",
                          "type": "number"
                        },
                        "weight_decay": {
                          "default": 0.1,
                          "description": "Weight decay coefficient",
                          "type": "number"
                        }
                      },
                      "type": "object"
                    }
                  },
                  "type": "object"
                },
                "max_grad_norm": {
                  "default": 1,
                  "description": "Maximum gradient norm for gradient clipping. Applies to all optimizer types.",
                  "type": "number"
                }
              },
              "type": "object"
            },
            "session_id": {
              "description": "ID of the training session",
              "type": "string"
            },
            "status": {
              "default": "TRAINING_SESSION_STATUS_UNSPECIFIED",
              "description": "Status of the training session",
              "enum": [
                "TRAINING_SESSION_STATUS_UNSPECIFIED",
                "TRAINING_SESSION_STATUS_CREATING",
                "TRAINING_SESSION_STATUS_RUNNING",
                "TRAINING_SESSION_STATUS_STOPPED",
                "TRAINING_SESSION_STATUS_STOPPING",
                "TRAINING_SESSION_STATUS_ERROR"
              ],
              "type": "string"
            },
            "updated_at": {
              "description": "Timestamp when the training session was last updated",
              "format": "date-time",
              "type": "string"
            }
          }
        },
        "type": "array"
      }
    },
    "default": {
      "description": "An unexpected error response.",
      "schema": {
        "properties": {
          "code": {
            "type": "integer"
          },
          "message": {
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "summary": "List training sessions",
  "tags": [
    "RL"
  ]
}
