                "type": "string"
              },
              "hash": {
                "title": "Hash",
                "type": "string"
              },
              "id": {
                "format": "uuid",
                "title": "Id",
                "type": "string"
              },
              "last_processed_at": {
                "anyOf": [
                  {
                    "format": "date-time",
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ],
                "title": "Last Processed At"
              },
              "library_id": {
                "format": "uuid",
                "title": "Library Id",
                "type": "string"
              },
              "mime_type": {
                "title": "Mime Type",
                "type": "string"
              },
              "name": {
                "title": "Name",
                "type": "string"
              },
              "number_of_pages": {
                "anyOf": [
                  {
                    "type": "integer"
                  },
                  {
                    "type": "null"
                  }
                ],
                "title": "Number Of Pages"
              },
              "processing_status": {
                "type": "string"
              },
              "size": {
                "title": "Size",
                "type": "integer"
              },
              "summary": {
                "anyOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ],
                "title": "Summary"
              },
