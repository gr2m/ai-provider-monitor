              },
              "tokens_processing_summary": {
                "anyOf": [
                  {
                    "type": "integer"
                  },
                  {
                    "type": "null"
                  }
                ],
                "title": "Tokens Processing Summary"
              },
              "tokens_processing_total": {
                "readOnly": true,
                "title": "Tokens Processing Total",
                "type": "integer"
              },
              "uploaded_by_id": {
                "format": "uuid",
                "title": "Uploaded By Id",
                "type": "string"
              },
              "uploaded_by_type": {
                "type": "string"
