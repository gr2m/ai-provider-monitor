{
  "description": "Get a paginated list of all files created by the user or organization associated with the provided API token.\n\nExample cURL request:\n\n```console\ncurl -s \\\n  -H \"Authorization: Token $REPLICATE_API_TOKEN\" \\\n  https://api.replicate.com/v1/files\n```\n\nThe response will be a paginated JSON array of file objects, sorted with the most recent file first.\n",
  "operationId": "files.list",
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "next": {
                "format": "uri",
                "nullable": true,
                "type": "string"
              },
              "previous": {
                "format": "uri",
                "nullable": true,
                "type": "string"
              },
              "results": {
                "items": {
                  "$ref": "#/components/schemas/schemas_file_response"
                },
                "type": "array"
              }
            },
            "type": "object"
          }
        }
      },
      "description": "Success"
    }
  },
  "summary": "List files",
  "components": {
    "schemas": {
      "schemas_file_response": {
        "description": "A file resource",
        "properties": {
          "checksums": {
            "description": "A dictionary of checksums for the file keyed by the algorithm name",
            "properties": {
              "sha256": {
                "description": "SHA256 checksum of the file",
                "example": "f047f8e745f9996c6e386064e06cb2007b8e6bc0a968a334fc0d2b2d64012066",
                "type": "string"
              }
            },
            "type": "object"
          },
          "content_type": {
            "description": "The content / MIME type of the file",
            "example": "application/zip",
            "type": "string"
          },
          "created_at": {
            "description": "When the file was created",
            "example": "2024-02-21T12:54:18.578761Z",
            "format": "date-time",
            "type": "string"
          },
          "expires_at": {
            "description": "When the file expires",
            "example": "2024-02-21T13:54:18.578761Z",
            "format": "date-time",
            "type": "string"
          },
          "id": {
            "description": "A unique, randomly-generated identifier for the file resource",
            "example": "cneqzikepnug6xezperrr4z55o",
            "type": "string"
          },
          "metadata": {
            "description": "Metadata provided by user when the file was created",
            "example": {
              "customer_reference_id": 123
            },
            "type": "object"
          },
          "size": {
            "description": "The length of the file in bytes",
            "example": 69420,
            "type": "integer"
          },
          "urls": {
            "description": "A dictionary of URLs associated with the file resource",
            "properties": {
              "get": {
                "description": "A URL to the file resource",
                "example": "https://api.replicate.com/v1/files/cneqzikepnug6xezperrr4z55o",
                "format": "uri",
                "type": "string"
              }
            },
            "type": "object"
          }
        },
        "required": [
          "id",
          "urls",
          "content_type",
          "size",
          "checksums",
          "metadata",
          "created_at",
          "expires_at"
        ],
        "type": "object"
      }
    }
  }
}
