{
  "operationId": "handle_video_generation_models_list_request",
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "example": {
            "models": [
              {
                "aliases": [],
                "created": 1743724800,
                "fingerprint": "fp_898ae9f31c",
                "id": "grok-imagine-video",
                "input_modalities": [
                  "text",
                  "image"
                ],
                "object": "model",
                "output_modalities": [
                  "video"
                ],
                "owned_by": "xai",
                "version": "1.0.0"
              }
            ]
          },
          "schema": {
            "$ref": "#/components/schemas/ListVideoGenerationModelsResponse"
          }
        }
      },
      "description": "Success"
    },
    "400": {
      "description": "Bad request. The request is invalid or an invalid API key is provided."
    }
  },
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "summary": "List all video generation models available to the authenticating API key with full information.",
  "tags": [
    "v1"
  ],
  "components": {
    "schemas": {
      "ListVideoGenerationModelsResponse": {
        "properties": {
          "models": {
            "description": "Array of available video generation models.",
            "items": {
              "$ref": "#/components/schemas/VideoGenerationModel"
            },
            "type": "array"
          }
        },
        "required": [
          "models"
        ],
        "type": "object"
      },
      "VideoGenerationModel": {
        "description": "Details of a video generation model",
        "properties": {
          "aliases": {
            "description": "Alias ID(s) of the model that user can use in a request's model field.",
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "created": {
            "description": "Model creation time in Unix timestamp.",
            "format": "int64",
            "type": "integer"
          },
          "fingerprint": {
            "description": "Fingerprint of the xAI system configuration hosting the model.",
            "type": "string"
          },
          "id": {
            "description": "Model ID.",
            "type": "string"
          },
          "input_modalities": {
            "description": "The input modalities supported by the model (e.g. \"text\", \"image\").",
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "object": {
            "description": "The object type, which is always `\"model\"`.",
            "type": "string"
          },
          "output_modalities": {
            "description": "The output modalities supported by the model (e.g. \"video\").",
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "owned_by": {
            "description": "Owner of the model.",
            "type": "string"
          },
          "version": {
            "description": "Version of the model.",
            "type": "string"
          }
        },
        "required": [
          "id",
          "fingerprint",
          "created",
          "object",
          "owned_by",
          "version",
          "input_modalities",
          "output_modalities",
          "aliases"
        ],
        "type": "object"
      }
    }
  }
}
