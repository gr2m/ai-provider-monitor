{
  "description": "Retrieves all API keys associated with the specified project",
  "operationId": "manage.v1.projects.keys.list",
  "parameters": [
    {
      "description": "Only return keys with a specific status",
      "in": "query",
      "name": "status",
      "required": false,
      "schema": {
        "enum": [
          "active",
          "expired"
        ],
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "api_keys": {
                "items": {
                  "properties": {
                    "api_key": {
                      "properties": {
                        "api_key_id": {
                          "example": "1234567890abcdef1234567890abcdef",
                          "type": "string"
                        },
                        "comment": {
                          "example": "A comment",
                          "type": "string"
                        },
                        "created": {
                          "example": "2021-01-01T00:00:00Z",
                          "format": "date-time",
                          "type": "string"
                        },
                        "scopes": {
                          "example": [
                            "admin"
                          ],
                          "items": {
                            "type": "string"
                          },
                          "type": "array"
                        }
                      },
                      "type": "object"
                    },
                    "member": {
                      "properties": {
                        "email": {
                          "example": "john@test.com",
                          "type": "string"
                        },
                        "member_id": {
                          "example": "1000-2000-3000-4000",
                          "type": "string"
                        }
                      },
                      "type": "object"
                    }
                  },
                  "type": "object"
                },
                "type": "array"
              }
            },
            "type": "object"
          }
        }
      },
      "description": "A list of API keys"
    },
    "400": {
      "content": {
        "application/json": {
          "schema": {
            "oneOf": [
              {
                "type": "string"
              },
              {
                "properties": {
                  "err_code": {
                    "description": "The error code",
                    "type": "string"
                  },
                  "err_msg": {
                    "description": "The error message",
                    "type": "string"
                  },
                  "request_id": {
                    "description": "The request ID",
                    "type": "string"
                  }
                },
                "type": "object"
              },
              {
                "properties": {
                  "category": {
                    "description": "The category of the error",
                    "type": "string"
                  },
                  "details": {
                    "description": "A description of the error",
                    "type": "string"
                  },
                  "message": {
                    "description": "A message about the error",
                    "type": "string"
                  },
                  "request_id": {
                    "description": "The unique identifier of the request",
                    "type": "string"
                  }
                },
                "type": "object"
              }
            ]
          }
        }
      },
      "description": "Invalid Request"
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    }
  ],
  "summary": "List Project Keys",
  "tags": [
    "Manage",
    "V1",
    "Projects",
    "Keys"
  ]
}
