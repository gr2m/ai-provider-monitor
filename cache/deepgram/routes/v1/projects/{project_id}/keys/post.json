{
  "description": "Creates a new API key with specified settings for the project",
  "operationId": "manage.projects.keys.create",
  "requestBody": {
    "content": {
      "application/json": {
        "schema": {
          "not": {
            "required": [
              "expiration_date",
              "time_to_live_in_seconds"
            ]
          },
          "oneOf": [
            {
              "required": [
                "expiration_date"
              ]
            },
            {
              "required": [
                "time_to_live_in_seconds"
              ]
            }
          ],
          "properties": {
            "comment": {
              "example": "A comment",
              "type": "string"
            },
            "expiration_date": {
              "example": "2026-01-01T00:00:00Z",
              "format": "date-time",
              "type": "string"
            },
            "scopes": {
              "example": [
                "admin"
              ],
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            "tags": {
              "example": [
                "prod",
                "west-region"
              ],
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            "time_to_live_in_seconds": {
              "example": 3600,
              "type": "number"
            }
          },
          "required": [
            "comment",
            "scopes"
          ],
          "type": "object"
        }
      }
    },
    "description": "API key settings",
    "required": true
  },
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "description": "API key created",
            "example": {
              "api_key_id": "1234567890abcdef1234567890abcdef",
              "comment": "a comment",
              "expiration_date": "2024-05-01T00:00:00.000000Z",
              "key": "1234567890abcdef1234567890abcdef",
              "scopes": [
                "member"
              ],
              "tags": [
                "tag-1"
              ]
            },
            "properties": {
              "api_key_id": {
                "description": "The unique identifier of the API key",
                "type": "string"
              },
              "comment": {
                "description": "A comment for the API key",
                "type": "string"
              },
              "expiration_date": {
                "description": "The expiration date of the API key",
                "format": "date-time",
                "type": "string"
              },
              "key": {
                "description": "The API key",
                "type": "string"
              },
              "scopes": {
                "description": "The scopes for the API key",
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              "tags": {
                "description": "The tags for the API key",
                "items": {
                  "type": "string"
                },
                "type": "array"
              }
            },
            "type": "object"
          }
        }
      },
      "description": "API key created successfully"
    },
    "400": {
      "content": {
        "application/json": {
          "schema": {
            "oneOf": [
              {
                "type": "string"
              },
              {
                "properties": {
                  "err_code": {
                    "description": "The error code",
                    "type": "string"
                  },
                  "err_msg": {
                    "description": "The error message",
                    "type": "string"
                  },
                  "request_id": {
                    "description": "The request ID",
                    "type": "string"
                  }
                },
                "type": "object"
              },
              {
                "properties": {
                  "category": {
                    "description": "The category of the error",
                    "type": "string"
                  },
                  "details": {
                    "description": "A description of the error",
                    "type": "string"
                  },
                  "message": {
                    "description": "A message about the error",
                    "type": "string"
                  },
                  "request_id": {
                    "description": "The unique identifier of the request",
                    "type": "string"
                  }
                },
                "type": "object"
              }
            ]
          }
        }
      },
      "description": "Invalid Request"
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    }
  ],
  "summary": "Create a Project Key",
  "tags": [
    "Manage",
    "V1",
    "Projects",
    "Keys"
  ]
}
