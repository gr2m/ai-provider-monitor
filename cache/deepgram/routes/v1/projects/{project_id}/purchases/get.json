{
  "description": "Returns the original purchased amount on an order transaction",
  "operationId": "manage.v1.projects.billing.purchases.list",
  "parameters": [
    {
      "description": "Number of results to return per page. Default 10. Range [1,1000]",
      "in": "query",
      "name": "limit",
      "required": false,
      "schema": {
        "default": 10,
        "maximum": 1000,
        "minimum": 1,
        "type": "number"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "orders": {
                "items": {
                  "properties": {
                    "amount": {
                      "example": 150,
                      "format": "float",
                      "type": "number"
                    },
                    "created": {
                      "example": "2023-02-21T21:13:40.014373Z",
                      "format": "date-time",
                      "type": "string"
                    },
                    "expiration": {
                      "example": "2026-03-04T00:00:00Z",
                      "format": "date-time",
                      "type": "string"
                    },
                    "order_id": {
                      "example": "025e19ba-b6d9-4a04-9f99-4fe715aca5f1",
                      "format": "uuid",
                      "type": "string"
                    },
                    "order_type": {
                      "example": "promotional",
                      "type": "string"
                    },
                    "units": {
                      "example": "usd",
                      "type": "string"
                    }
                  },
                  "type": "object"
                },
                "type": "array"
              }
            },
            "type": "object"
          }
        }
      },
      "description": "A list of purchases for a specific project"
    },
    "400": {
      "content": {
        "application/json": {
          "schema": {
            "oneOf": [
              {
                "type": "string"
              },
              {
                "properties": {
                  "err_code": {
                    "description": "The error code",
                    "type": "string"
                  },
                  "err_msg": {
                    "description": "The error message",
                    "type": "string"
                  },
                  "request_id": {
                    "description": "The request ID",
                    "type": "string"
                  }
                },
                "type": "object"
              },
              {
                "properties": {
                  "category": {
                    "description": "The category of the error",
                    "type": "string"
                  },
                  "details": {
                    "description": "A description of the error",
                    "type": "string"
                  },
                  "message": {
                    "description": "A message about the error",
                    "type": "string"
                  },
                  "request_id": {
                    "description": "The unique identifier of the request",
                    "type": "string"
                  }
                },
                "type": "object"
              }
            ]
          }
        }
      },
      "description": "Invalid Request"
    }
