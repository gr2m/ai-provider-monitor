      "schema": {
        "default": false,
        "type": "boolean"
      }
    },
    {
      "description": "Summarize content. For Listen API, supports string version option. For Read API, accepts boolean only.",
      "in": "query",
      "name": "summarize",
      "required": false,
      "schema": {
        "oneOf": [
          {
            "enum": [
              "v2"
            ],
            "title": "Listen API Version Option",
            "type": "string"
          },
          {
            "default": false,
            "title": "Read API Boolean Option",
            "type": "boolean"
          }
        ]
      }
    },
    {
