{
  "description": "Returns metadata on all the latest public models. To retrieve custom models, use Get Project Models.",
  "operationId": "manage.v1.models.list",
  "parameters": [
    {
      "$ref": "#/components/parameters/ManageV1IncludeOutdatedModels"
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/ListModelsV1Response"
          }
        }
      },
      "description": "A list of models"
    },
    "400": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/ErrorResponse"
          }
        }
      },
      "description": "Invalid Request"
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    }
  ],
  "summary": "List Models",
  "tags": [
    "Manage",
    "V1",
    "Models"
  ],
  "components": {
    "parameters": {
      "ManageV1IncludeOutdatedModels": {
        "description": "returns non-latest versions of models",
        "in": "query",
        "name": "include_outdated",
        "required": false,
        "schema": {
          "type": "boolean"
        }
      }
    },
    "schemas": {
      "ListModelsV1Response": {
        "properties": {
          "stt": {
            "items": {
              "$ref": "#/components/schemas/ListModelsV1ResponseSttModels"
            },
            "type": "array"
          },
          "tts": {
            "items": {
              "$ref": "#/components/schemas/ListModelsV1ResponseTtsModels"
            },
            "type": "array"
          }
        },
        "type": "object"
      },
      "ErrorResponse": {
        "oneOf": [
          {
            "$ref": "#/components/schemas/ErrorResponseTextError"
          },
          {
            "$ref": "#/components/schemas/ErrorResponseLegacyError"
          },
          {
            "$ref": "#/components/schemas/ErrorResponseModernError"
          }
        ]
      },
      "ListModelsV1ResponseSttModels": {
        "properties": {
          "architecture": {
            "example": "base",
            "type": "string"
          },
          "batch": {
            "example": true,
            "type": "boolean"
          },
          "canonical_name": {
            "example": "nova-3",
            "type": "string"
          },
          "formatted_output": {
            "example": true,
            "type": "boolean"
          },
          "languages": {
            "example": [
              "en",
              "en-us"
            ],
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "name": {
            "example": "nova-3",
            "type": "string"
          },
          "streaming": {
            "example": true,
            "type": "boolean"
          },
          "uuid": {
            "example": "6b28e919-8427-4f32-9847-492e2efd7daf",
            "type": "string"
          },
          "version": {
            "example": "2021-11-10.1",
            "type": "string"
          }
        },
        "type": "object"
      },
      "ListModelsV1ResponseTtsModels": {
        "properties": {
          "architecture": {
            "example": "aura-2",
            "type": "string"
          },
          "canonical_name": {
            "example": "aura-2-zeus-en",
            "type": "string"
          },
          "languages": {
            "example": [
              "en",
              "en-US"
            ],
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "metadata": {
            "properties": {
              "accent": {
                "example": "American",
                "type": "string"
              },
              "age": {
                "example": "Adult",
                "type": "string"
              },
              "color": {
                "example": "#C58DFF",
                "type": "string"
              },
              "image": {
                "example": "https://static.deepgram.com/examples/avatars/zeus.jpg",
                "format": "uri",
                "type": "string"
              },
              "sample": {
                "example": "https://static.deepgram.com/examples/Aura-2-zeus.wav",
                "format": "uri",
                "type": "string"
              },
              "tags": {
                "example": [
                  "masculine",
                  "deep",
                  "trustworthy",
                  "smooth"
                ],
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              "use_cases": {
                "example": [
                  "IVR"
                ],
                "items": {
                  "type": "string"
                },
                "type": "array"
              }
            },
            "type": "object"
          },
          "name": {
            "example": "zeus",
            "type": "string"
          },
          "uuid": {
            "example": "2baf189d-91ac-481d-b6d1-750888667b31",
            "format": "uuid",
            "type": "string"
          },
          "version": {
            "example": "2025-04-07.0",
            "type": "string"
          }
        },
        "type": "object"
      },
      "ErrorResponseTextError": {
        "type": "string"
      },
      "ErrorResponseLegacyError": {
        "properties": {
          "err_code": {
            "description": "The error code",
            "type": "string"
          },
          "err_msg": {
            "description": "The error message",
            "type": "string"
          },
          "request_id": {
            "description": "The request ID",
            "type": "string"
          }
        },
        "type": "object"
      },
      "ErrorResponseModernError": {
        "properties": {
          "category": {
            "description": "The category of the error",
            "type": "string"
          },
          "details": {
            "description": "A description of the error",
            "type": "string"
          },
          "message": {
            "description": "A message about the error",
            "type": "string"
          },
          "request_id": {
            "description": "The unique identifier of the request",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  }
}
