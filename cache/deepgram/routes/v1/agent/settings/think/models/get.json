{
  "description": "Retrieves the available think models that can be used for AI agent processing",
  "operationId": "agent.v1.settings.think.models.list",
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "properties": {
              "models": {
                "items": {
                  "oneOf": [
                    {
                      "description": "OpenAI models",
                      "properties": {
                        "id": {
                          "description": "The unique identifier of the OpenAI model",
                          "enum": [
                            "gpt-5",
                            "gpt-5-mini",
                            "gpt-5-nano",
                            "gpt-4.1",
                            "gpt-4.1-mini",
                            "gpt-4.1-nano",
                            "gpt-4o",
                            "gpt-4o-mini"
                          ],
                          "type": "string"
                        },
                        "name": {
                          "description": "The display name of the model",
                          "type": "string"
                        },
                        "provider": {
                          "const": "open_ai",
                          "description": "The provider of the model"
                        }
                      },
                      "required": [
                        "id",
                        "name",
                        "provider"
                      ],
                      "type": "object"
                    },
                    {
                      "description": "Anthropic models",
                      "properties": {
                        "id": {
                          "description": "The unique identifier of the Anthropic model",
                          "enum": [
                            "claude-3-5-haiku-latest",
                            "claude-sonnet-4-20250514"
                          ],
                          "type": "string"
                        },
                        "name": {
                          "description": "The display name of the model",
                          "type": "string"
                        },
                        "provider": {
                          "const": "anthropic",
                          "description": "The provider of the model"
                        }
                      },
                      "required": [
                        "id",
                        "name",
                        "provider"
                      ],
                      "type": "object"
                    },
                    {
                      "description": "Google models",
                      "properties": {
                        "id": {
                          "description": "The unique identifier of the Google model",
                          "enum": [
                            "gemini-2.5-flash",
                            "gemini-2.0-flash",
                            "gemini-2.0-flash-lite"
                          ],
                          "type": "string"
                        },
                        "name": {
                          "description": "The display name of the model",
                          "type": "string"
                        },
                        "provider": {
                          "const": "google",
                          "description": "The provider of the model"
                        }
                      },
                      "required": [
                        "id",
                        "name",
                        "provider"
                      ],
                      "type": "object"
                    },
                    {
                      "description": "Groq models",
                      "properties": {
                        "id": {
                          "description": "The unique identifier of the Groq model",
                          "enum": [
                            "openai/gpt-oss-20b"
                          ],
                          "type": "string"
                        },
                        "name": {
                          "description": "The display name of the model",
                          "type": "string"
                        },
                        "provider": {
                          "const": "groq",
                          "description": "The provider of the model"
                        }
                      },
                      "required": [
                        "id",
                        "name",
                        "provider"
                      ],
                      "type": "object"
                    },
                    {
                      "description": "AWS Bedrock models (custom models accepted)",
                      "properties": {
                        "id": {
                          "description": "The unique identifier of the AWS Bedrock model (any model string accepted for BYO LLMs)",
                          "type": "string"
                        },
                        "name": {
                          "description": "The display name of the model",
                          "type": "string"
                        },
                        "provider": {
                          "const": "aws_bedrock",
                          "description": "The provider of the model"
                        }
                      },
                      "required": [
                        "id",
                        "name",
                        "provider"
                      ],
                      "type": "object"
                    }
                  ]
                },
                "type": "array"
              }
            },
            "required": [
              "models"
            ],
            "type": "object"
          }
        }
      },
      "description": "List of available think models"
    },
    "400": {
      "content": {
        "application/json": {
          "schema": {
            "oneOf": [
              {
                "type": "string"
              },
              {
                "properties": {
                  "err_code": {
                    "description": "The error code",
                    "type": "string"
                  },
                  "err_msg": {
                    "description": "The error message",
                    "type": "string"
                  },
                  "request_id": {
                    "description": "The request ID",
                    "type": "string"
                  }
                },
                "type": "object"
              },
              {
                "properties": {
                  "category": {
                    "description": "The category of the error",
                    "type": "string"
                  },
                  "details": {
                    "description": "A description of the error",
                    "type": "string"
                  },
                  "message": {
                    "description": "A message about the error",
                    "type": "string"
                  },
                  "request_id": {
                    "description": "The unique identifier of the request",
                    "type": "string"
                  }
                },
                "type": "object"
              }
            ]
          }
        }
      },
      "description": "Invalid Request"
    }
  },
  "summary": "List Agent Think Models",
  "tags": [
    "Agent",
    "V1",
    "Settings",
    "Think",
    "Models"
  ]
}
