{
  "description": "Returns a list of all compute instances belonging to the authenticated user's workspace.\n\n**Requirements:**\n- Requires compute permissions (extra_permissions.compute = true)\n- Authentication required via admin API key\n\n**Key Features:**\n- View all instances regardless of status\n- Includes instance configuration, region, and current status\n- Paginated results for large instance lists\n\n**Common Use Cases:**\n- Monitor all active compute resources\n- Check instance status and availability\n- Audit compute resource usage\n- Build compute management dashboards\n\nSee [fal.ai docs](https://docs.fal.ai/compute) for more details.",
  "operationId": "listComputeInstances",
  "parameters": [
    {
      "description": "Maximum number of items to return. Actual maximum depends on query type and expansion parameters.",
      "in": "query",
      "name": "limit",
      "required": false,
      "schema": {
        "description": "Maximum number of items to return. Actual maximum depends on query type and expansion parameters.",
        "example": 50,
        "minimum": 1,
        "type": "integer"
      }
    },
    {
      "description": "Pagination cursor from previous response. Encodes the page number.",
      "in": "query",
      "name": "cursor",
      "required": false,
      "schema": {
        "description": "Pagination cursor from previous response. Encodes the page number.",
        "example": "Mg==",
        "type": "string"
      }
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "example": {
            "has_more": false,
            "instances": [
              {
                "creator_user_nickname": "developer",
                "id": "inst_abc123xyz",
                "instance_type": "gpu_1x_h100_sxm5",
                "ip": "203.0.113.42",
                "region": "us-west",
                "sector": "sector_1",
                "status": "ready"
              },
              {
                "creator_user_nickname": "developer",
                "id": "inst_def456uvw",
                "instance_type": "gpu_8x_h100_sxm5",
                "region": "us-east",
                "status": "provisioning"
              }
            ],
            "next_cursor": null
          },
          "schema": {
            "description": "Response containing a list of compute instances with pagination support",
            "properties": {
              "has_more": {
                "description": "Boolean indicating if more results are available (convenience field derived from next_cursor)",
                "type": "boolean"
              },
              "instances": {
                "description": "Array of compute instances belonging to the authenticated user",
                "items": {
                  "description": "Compute instance details including configuration, location, and status",
                  "properties": {
                    "creator_user_nickname": {
                      "description": "Nickname of the user who created this instance",
                      "example": "developer",
                      "type": "string"
                    },
                    "id": {
                      "description": "Unique identifier for the compute instance",
                      "example": "inst_abc123xyz",
                      "type": "string"
                    },
                    "instance_type": {
                      "description": "Type of compute instance (GPU configuration)",
                      "enum": [
                        "gpu_8x_h100_sxm5",
                        "gpu_1x_h100_sxm5"
                      ],
                      "example": "gpu_1x_h100_sxm5",
                      "type": "string"
                    },
                    "ip": {
                      "description": "IP address of the instance (available when instance is ready)",
                      "example": "203.0.113.42",
                      "type": "string"
                    },
                    "region": {
                      "description": "Geographical region where the instance is located",
                      "enum": [
                        "us-west",
                        "us-central",
                        "us-east",
                        "eu-north",
                        "eu-west",
                        "other"
                      ],
                      "example": "us-west",
                      "type": "string"
                    },
                    "sector": {
                      "description": "Sector identifier for instance placement within the region (if applicable)",
                      "enum": [
                        "sector_1",
                        "sector_2",
                        "sector_3"
                      ],
                      "example": "sector_1",
                      "type": "string"
                    },
                    "status": {
                      "description": "Current operational status of the instance",
                      "enum": [
                        "ready",
                        "init",
                        "pending",
                        "provisioning",
                        "stopped",
                        "unknown"
                      ],
                      "example": "ready",
                      "type": "string"
                    }
                  },
                  "required": [
                    "id",
                    "instance_type",
                    "region",
                    "status"
                  ],
                  "type": "object"
                },
                "type": "array"
              },
              "next_cursor": {
                "description": "Cursor for the next page of results, null if no more pages",
                "type": [
                  "string",
                  "null"
                ]
              }
            },
            "required": [
              "next_cursor",
              "has_more",
              "instances"
            ],
            "type": "object"
          }
        }
      },
      "description": "Successfully retrieved compute instances"
    },
