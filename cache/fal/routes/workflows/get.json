{
  "description": "\nList workflows for the authenticated user with optional search and filtering.\n\n**Features:**\n- Paginated results with cursor-based pagination\n- Search by workflow name or title\n- Filter by model endpoints used in the workflow\n\n**Authentication:** Required. Returns only workflows owned by the authenticated user.\n\n**Common Use Cases:**\n- Display user's workflow library\n- Search for specific workflows\n- Find workflows using particular models\n    ",
  "operationId": "listWorkflows",
  "parameters": [
    {
      "description": "Maximum number of items to return. Actual maximum depends on query type and expansion parameters.",
      "in": "query",
      "name": "limit",
      "required": false,
      "schema": {
        "description": "Maximum number of items to return. Actual maximum depends on query type and expansion parameters.",
        "example": 50,
        "minimum": 1,
        "type": "integer"
      }
    },
    {
      "description": "Pagination cursor from previous response. Encodes the page number.",
      "in": "query",
      "name": "cursor",
      "required": false,
      "schema": {
        "description": "Pagination cursor from previous response. Encodes the page number.",
        "example": "Mg==",
        "type": "string"
      }
    },
    {
      "description": "Search by workflow name or title",
      "in": "query",
      "name": "search",
      "required": false,
      "schema": {
        "description": "Search by workflow name or title",
        "example": "image generation",
        "type": "string"
      }
    },
    {
      "description": "Filter by model endpoint IDs used in the workflow. Can be a single value or comma-separated values.",
      "explode": true,
      "in": "query",
      "name": "used_endpoint_ids",
      "required": false,
      "schema": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        ],
        "description": "Filter by model endpoint IDs used in the workflow. Can be a single value or comma-separated values.",
        "example": [
          "fal-ai/flux/dev"
        ]
      },
      "style": "form"
    }
  ],
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "example": {
            "has_more": false,
            "next_cursor": null,
            "total": 1,
            "workflows": [
              {
                "created_at": "2025-01-15T12:00:00Z",
                "description": "A workflow for generating images",
                "endpoint_ids": [
                  "fal-ai/flux/dev"
                ],
                "name": "my-image-workflow",
                "tags": [
                  "image",
                  "generation"
                ],
                "thumbnail_url": "https://fal.media/files/example.jpg",
                "title": "My Image Generation Workflow",
                "user_nickname": "johndoe"
              }
            ]
          },
          "schema": {
            "description": "Response containing workflow data with pagination support",
            "properties": {
              "has_more": {
                "description": "Whether more results are available",
                "example": true,
                "type": "boolean"
              },
              "next_cursor": {
                "description": "Cursor for the next page of results, null if no more pages",
                "example": "eyJvZmZzZXQiOjEwfQ==",
                "type": [
                  "string",
                  "null"
                ]
              },
              "total": {
                "description": "Total number of workflows matching the query",
                "example": 42,
                "type": "integer"
              },
              "workflows": {
                "description": "Array of workflow items",
                "example": [
                  {
                    "created_at": "2024-01-15T10:30:00Z",
                    "description": "Generate high-quality images using FLUX model",
                    "endpoint_ids": [
                      "fal-ai/flux/dev",
                      "fal-ai/face-swap"
                    ],
                    "name": "my-image-workflow",
                    "tags": [
                      "image-generation",
                      "ai",
                      "flux"
                    ],
                    "thumbnail_url": "https://fal.ai/workflows/thumb_abc123.png",
                    "title": "My Image Generation Workflow",
                    "user_nickname": "johndoe"
                  }
                ],
                "items": {
                  "description": "Workflow information",
                  "properties": {
                    "created_at": {
                      "description": "ISO8601 timestamp of workflow creation",
                      "example": "2024-01-15T10:30:00Z",
                      "type": "string"
                    },
                    "description": {
                      "description": "Brief description of what the workflow does",
                      "example": "Generate high-quality images using FLUX model",
                      "type": "string"
                    },
                    "endpoint_ids": {
                      "description": "List of model endpoint IDs used in this workflow",
                      "example": [
                        "fal-ai/flux/dev",
                        "fal-ai/face-swap"
                      ],
                      "items": {
                        "example": "fal-ai/flux/dev",
                        "type": "string"
                      },
                      "type": "array"
                    },
                    "name": {
                      "description": "Unique workflow name/slug within the user's namespace",
                      "example": "my-image-workflow",
                      "type": "string"
                    },
                    "tags": {
                      "description": "Tags associated with the workflow",
                      "example": [
                        "image-generation",
                        "ai",
                        "flux"
                      ],
                      "items": {
                        "example": "image-generation",
                        "type": "string"
                      },
                      "type": "array"
                    },
                    "thumbnail_url": {
                      "description": "URL to the workflow thumbnail image",
                      "example": "https://fal.ai/workflows/thumb_abc123.png",
                      "type": "string"
                    },
                    "title": {
                      "description": "Human-readable workflow title",
                      "example": "My Image Generation Workflow",
                      "type": "string"
                    },
                    "user_nickname": {
                      "description": "Display name/username of the owner",
                      "example": "johndoe",
                      "type": "string"
                    }
                  },
                  "required": [
                    "name",
                    "title",
                    "user_nickname",
                    "created_at",
                    "tags",
                    "endpoint_ids"
                  ],
                  "type": "object"
                },
                "type": "array"
              }
            },
            "required": [
              "workflows",
              "next_cursor",
              "has_more"
            ],
            "type": "object"
          }
        }
      },
      "description": "Successfully retrieved workflows"
    },
