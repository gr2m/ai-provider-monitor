      "schema": {
        "description": "Target path (including filename)",
        "example": "datasets/images/cat.jpg",
        "type": "string"
      }
    },
    {
      "description": "If true and the uploaded file is a ZIP, it will be extracted",
      "in": "query",
      "name": "unzip",
      "required": false,
      "schema": {
        "default": false,
        "description": "If true and the uploaded file is a ZIP, it will be extracted",
        "example": false,
        "type": [
          "boolean",
          "null"
        ]
