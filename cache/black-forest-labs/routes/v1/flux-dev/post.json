{
  "description": "Submits an image generation task with FLUX.1 [dev].",
  "operationId": "flux_dev_v1_flux_dev_post",
  "requestBody": {
    "content": {
      "application/json": {
        "schema": {
          "$ref": "#/components/schemas/FluxDevInputs"
        }
      }
    },
    "required": true
  },
  "responses": {
    "200": {
      "content": {
        "application/json": {
          "schema": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/AsyncResponse"
              },
              {
                "$ref": "#/components/schemas/AsyncWebhookResponse"
              }
            ],
            "title": "Response Flux Dev V1 Flux Dev Post"
          }
        }
      },
      "description": "Successful Response"
    },
    "422": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/HTTPValidationError"
          }
        }
      },
      "description": "Validation Error"
    }
  },
  "security": [
    {
      "APIKeyHeader": []
    }
  ],
  "summary": "Generate an image with FLUX.1 [dev].",
  "tags": [
    "Models"
  ],
  "components": {
    "schemas": {
      "FluxDevInputs": {
        "properties": {
          "guidance": {
            "anyOf": [
              {
                "maximum": 5,
                "minimum": 1.5,
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "default": 3,
            "description": "Guidance scale for image generation. High guidance scales improve prompt adherence at the cost of reduced realism.",
            "example": 3,
            "title": "Guidance"
          },
          "height": {
            "default": 768,
            "description": "Height of the generated image in pixels. Must be a multiple of 32.",
            "maximum": 1440,
            "minimum": 256,
            "multipleOf": 32,
            "title": "Height",
            "type": "integer"
          },
          "image_prompt": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional base64 encoded image to use as a prompt for generation.",
            "title": "Image Prompt"
          },
          "output_format": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/OutputFormat"
              },
              {
                "type": "null"
              }
            ],
            "default": "jpeg",
            "description": "Output format for the generated image. Can be 'jpeg' or 'png'."
          },
          "prompt": {
            "default": "",
            "description": "Text prompt for image generation.",
            "example": "ein fantastisches bild",
            "title": "Prompt",
            "type": "string"
          },
          "prompt_upsampling": {
            "default": false,
            "description": "Whether to perform upsampling on the prompt. If active, automatically modifies the prompt for more creative generation.",
            "title": "Prompt Upsampling",
            "type": "boolean"
          },
          "safety_tolerance": {
            "default": 2,
            "description": "Tolerance level for input and output moderation. Between 0 and 6, 0 being most strict, 6 being least strict.",
            "example": 2,
            "maximum": 6,
            "minimum": 0,
            "title": "Safety Tolerance",
            "type": "integer"
          },
          "seed": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional seed for reproducibility.",
            "example": 42,
            "title": "Seed"
          },
          "steps": {
            "anyOf": [
              {
                "maximum": 50,
                "minimum": 1,
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "default": 28,
            "description": "Number of steps for the image generation process.",
            "example": 28,
            "title": "Steps"
          },
          "webhook_secret": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional secret for webhook signature verification",
            "title": "Webhook Secret"
          },
          "webhook_url": {
            "anyOf": [
              {
                "format": "uri",
                "maxLength": 2083,
                "minLength": 1,
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "URL to receive webhook notifications",
            "title": "Webhook Url"
          },
          "width": {
            "default": 1024,
            "description": "Width of the generated image in pixels. Must be a multiple of 32.",
            "maximum": 1440,
            "minimum": 256,
            "multipleOf": 32,
            "title": "Width",
            "type": "integer"
          }
        },
        "title": "FluxDevInputs",
        "type": "object"
      },
      "AsyncResponse": {
        "properties": {
          "cost": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "description": "Cost in credits for this request",
            "title": "Cost"
          },
          "id": {
            "title": "Id",
            "type": "string"
          },
          "input_mp": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "description": "Input megapixels (2 decimal places)",
            "title": "Input Mp"
          },
          "output_mp": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "description": "Output megapixels (2 decimal places)",
            "title": "Output Mp"
          },
          "polling_url": {
            "title": "Polling Url",
            "type": "string"
          }
        },
        "required": [
          "id",
          "polling_url"
        ],
        "title": "AsyncResponse",
        "type": "object"
      },
      "AsyncWebhookResponse": {
        "properties": {
          "cost": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "description": "Cost in credits for this request",
            "title": "Cost"
          },
          "id": {
            "title": "Id",
            "type": "string"
          },
          "input_mp": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "description": "Input megapixels (2 decimal places)",
            "title": "Input Mp"
          },
          "output_mp": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "description": "Output megapixels (2 decimal places)",
            "title": "Output Mp"
          },
          "status": {
            "title": "Status",
            "type": "string"
          },
          "webhook_url": {
            "title": "Webhook Url",
            "type": "string"
          }
        },
        "required": [
          "id",
          "status",
          "webhook_url"
        ],
        "title": "AsyncWebhookResponse",
        "type": "object"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "title": "Detail",
            "type": "array"
          }
        },
        "title": "HTTPValidationError",
        "type": "object"
      },
      "OutputFormat": {
        "enum": [
          "jpeg",
          "png"
        ],
        "title": "OutputFormat",
        "type": "string"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "title": "Location",
            "type": "array"
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        },
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError",
        "type": "object"
      }
    }
  }
}
